<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="GT RWD" type="template">

  <section name="Driver">
    <!-- Position of the driver -->
    <attnum name="xpos" val="0.4" unit="m"></attnum>
    <attnum name="ypos" val="0.0" unit="m"></attnum>
    <attnum name="zpos" val="1.2" unit="m"></attnum>
  </section>

  <section name="Car">
    <attstr name="category" val="Track-RWD-GrB"></attstr>    
    <attnum name="body length" unit="m" val="4.5"></attnum>
    <attnum name="body width"  unit="m" val="2.0"></attnum>
    <attnum name="body height" unit="m" val="1.2"></attnum>
    <!-- collision bounding box -->
    <attnum name="overall length" unit="m" val="4.5"></attnum>
    <attnum name="overall width"  unit="m" val="2.0"></attnum>
    <attnum name="GC height" unit="m" val="0.20"></attnum>
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" val="0.4"></attnum>
    <attnum name="front right-left weight repartition" min="0.4" max="0.6" val="0.5"></attnum>
    <attnum name="rear right-left weight repartition" min="0.4" max="0.6" val="0.5"></attnum>
    <!-- used for inertia, smaller values indicate better mass centering -->
    <attnum name="mass repartition coefficient" val="0.6"></attnum>
    <attnum name="fuel tank" unit="l" val="100.0"></attnum>
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="100.0"></attnum>
  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" val="0.3"></attnum>
    <attnum name="front area" unit="m2" val="1.7"></attnum>
    <attnum name="Clift" val="1.0"></attnum>
  </section>

  <section name="Front Wing">
    <attnum name="area" unit="m2" val="0.5"></attnum>
    <attnum name="angle" unit="deg" min="0" max="30" val="30"></attnum>
    <attnum name="xpos" unit="m" val="2"></attnum>
    <attnum name="zpos" unit="m" val="0.1"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="area"  unit="m2" val="1.0"></attnum>
    <attnum name="angle" unit="deg" min="0" max="30" val="30"></attnum>
    <attnum name="xpos" unit="m" val="-2.3"></attnum>
    <attnum name="zpos" unit="m" val=".6"></attnum>
  </section>


  <section name="Gearbox">
    <attnum name="shift time" unit="s" val="0.15"></attnum>
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"></attnum>
	<attnum name="inertia" val="0.0037"></attnum>
	<attnum name="efficiency" val="0.957"></attnum>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="2.923"></attnum>
	<attnum name="inertia" val="0.003"></attnum>
	<attnum name="efficiency" val="0.955"></attnum>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="1.75"></attnum>
	<attnum name="inertia" val="0.0037"></attnum>
	<attnum name="efficiency" val="0.957"></attnum>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.307"></attnum>
	<attnum name="inertia" val="0.0048"></attnum>
	<attnum name="efficiency" val="0.950"></attnum>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.033"></attnum>
	<attnum name="inertia" val="0.0064"></attnum>
	<attnum name="efficiency" val="0.983"></attnum>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.848"></attnum>
	<attnum name="inertia" val="0.0107"></attnum>
	<attnum name="efficiency" val="0.948"></attnum>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.8"></attnum>
	<attnum name="inertia" val="0.0150"></attnum>
	<attnum name="efficiency" val="0.930"></attnum>
      </section>
    </section>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="1" max="45" val="45"></attnum>
    <attnum name="max steer speed" unit="deg/s" min="1" max="360" val="120"></attnum>
  </section>

  <section name="Drivetrain">
    <attstr name="type"  val="RWD"></attstr>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE" val="FREE"></attstr>
    <attnum name="ratio" min="0" max="15" val="4.2"></attnum>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.35"></attnum>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="11000"></attnum>
  </section>

  <section name="Front Axle">
    <attnum name="xpos" unit="m" val="1.23"></attnum>
    <attnum name="roll center height" unit="m" val="0.012"></attnum>
  </section>

  <section name="Rear Axle">
    <attnum name="xpos" unit="m" val="-1.52"></attnum>
    <attnum name="roll center height" unit="m" val="0.04"></attnum>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ypos" unit="m" val="-0.78"></attnum>
    <attnum name="rim diameter" val="18.0" unit="in" min="16"></attnum>
    <attnum name="tire width" unit="mm" val="295"></attnum>
    <attnum name="tire height-width ratio" val=".35"></attnum>
    <attnum name="mu" min="0.05" max="1.6" val="1.6"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="50" max="200" val="80"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ypos" unit="m" val="0.78"></attnum>
    <attnum name="rim diameter" unit="in" val="18.0" min="16"></attnum>
    <attnum name="tire width" unit="mm" val="295"></attnum>
    <attnum name="tire height-width ratio" val=".35"></attnum>
    <attnum name="mu" min="0.05" max="1.6" val="1.6"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="50" max="200" val="80"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ypos" unit="m" val="-0.85"></attnum>
    <attnum name="rim diameter" unit="in" val="18.0" min="16"></attnum>
    <attnum name="tire width" unit="mm" val="335"></attnum>
    <attnum name="tire height-width ratio" val=".30"></attnum>
    <attnum name="mu" min="0.05" max="1.6" val="1.6"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="50" max="200" val="80"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ypos" unit="m" val="0.85"></attnum>
    <attnum name="rim diameter" unit="in" val="18.0" min="16"></attnum>
    <attnum name="tire width" unit="mm" val="335"></attnum>
    <attnum name="tire height-width ratio" val=".30"></attnum>
    <attnum name="mu" min="0.05" max="1.6" val="1.6"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="50" max="200" val="80"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="2000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1"></attnum>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="2000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1.5"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
  </section>

  <section name="Front Right Brake">
    <attnum name="disk diameter" unit="mm" val="380"></attnum>
  </section>

  <section name="Front Left Brake">
    <attnum name="disk diameter" unit="mm" val="380"></attnum>
  </section>

  <section name="Rear Right Brake">
    <attnum name="disk diameter" unit="mm" val="280"></attnum>
  </section>

  <section name="Rear Left Brake">
    <attnum name="disk diameter" unit="mm" val="280"></attnum>
  </section>

</params>


