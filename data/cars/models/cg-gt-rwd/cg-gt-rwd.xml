<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="GT RWD" type="template">

  <section name="Driver">
    <!-- Position of the driver -->
    <attnum name="xpos" val="0.4" unit="m"/>
    <attnum name="ypos" val="0.0" unit="m"/>
    <attnum name="zpos" val="1.2" unit="m"/>
  </section>

  <section name="Car">
    <attstr name="category" val="Track-RWD-GrB"/>    
    <attnum name="body length" unit="m" val="4.5"/>
    <attnum name="body width"  unit="m" val="2.0"/>
    <attnum name="body height" unit="m" val="1.2"/>
    <!-- collision bounding box -->
    <attnum name="overall length" unit="m" val="4.5"/>
    <attnum name="overall width"  unit="m" val="2.0"/>
    <attnum name="GC height" unit="m" val="0.20"/>
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" val="0.4"/>
    <attnum name="front right-left weight repartition" min="0.4" max="0.6" val="0.5"/>
    <attnum name="rear right-left weight repartition" min="0.4" max="0.6" val="0.5"/>
    <!-- used for inertia, smaller values indicate better mass centering -->
    <attnum name="mass repartition coefficient" val="0.6"/>
    <attnum name="fuel tank" unit="l" val="100.0"/>
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="100.0"/>
  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" val="0.3"/>
    <attnum name="front area" unit="m2" val="1.7"/>
    <attnum name="Clift" val="1.0"/>
  </section>

  <section name="Front Wing">
    <attnum name="area" unit="m2" val="0.5"/>
    <attnum name="angle" unit="deg" min="0" max="30" val="30"/>
    <attnum name="xpos" unit="m" val="2"/>
    <attnum name="zpos" unit="m" val="0.1"/>
  </section>

  <section name="Rear Wing">
    <attnum name="area"  unit="m2" val="1.0"/>
    <attnum name="angle" unit="deg" min="0" max="30" val="30"/>
    <attnum name="xpos" unit="m" val="-2.3"/>
    <attnum name="zpos" unit="m" val=".6"/>
  </section>


  <section name="Gearbox">
    <attnum name="shift time" unit="s" val="0.15"/>
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"/>
	<attnum name="inertia" val="0.0037"/>
	<attnum name="efficiency" val="0.957"/>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="2.923"/>
	<attnum name="inertia" val="0.003"/>
	<attnum name="efficiency" val="0.955"/>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="1.75"/>
	<attnum name="inertia" val="0.0037"/>
	<attnum name="efficiency" val="0.957"/>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.307"/>
	<attnum name="inertia" val="0.0048"/>
	<attnum name="efficiency" val="0.950"/>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.033"/>
	<attnum name="inertia" val="0.0064"/>
	<attnum name="efficiency" val="0.983"/>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.848"/>
	<attnum name="inertia" val="0.0107"/>
	<attnum name="efficiency" val="0.948"/>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.8"/>
	<attnum name="inertia" val="0.0150"/>
	<attnum name="efficiency" val="0.930"/>
      </section>
    </section>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="1" max="45" val="45"/>
    <attnum name="max steer speed" unit="deg/s" min="1" max="360" val="120"/>
  </section>

  <section name="Drivetrain">
    <attstr name="type"  val="RWD"/>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE" val="FREE"/>
    <attnum name="ratio" min="0" max="15" val="4.2"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.35"/>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="11000"/>
  </section>

  <section name="Front Axle">
    <attnum name="xpos" unit="m" val="1.23"/>
    <attnum name="roll center height" unit="m" val="0.012"/>
  </section>

  <section name="Rear Axle">
    <attnum name="xpos" unit="m" val="-1.52"/>
    <attnum name="roll center height" unit="m" val="0.04"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ypos" unit="m" val="-0.78"/>
    <attnum name="rim diameter" val="18.0" unit="in" min="16"/>
    <attnum name="tire width" unit="mm" val="295"/>
    <attnum name="tire height-width ratio" val=".35"/>
    <attnum name="mu" min="0.05" max="1.6" val="1.6"/>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="50" max="200" val="80"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ypos" unit="m" val="0.78"/>
    <attnum name="rim diameter" unit="in" val="18.0" min="16"/>
    <attnum name="tire width" unit="mm" val="295"/>
    <attnum name="tire height-width ratio" val=".35"/>
    <attnum name="mu" min="0.05" max="1.6" val="1.6"/>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="50" max="200" val="80"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ypos" unit="m" val="-0.85"/>
    <attnum name="rim diameter" unit="in" val="18.0" min="16"/>
    <attnum name="tire width" unit="mm" val="335"/>
    <attnum name="tire height-width ratio" val=".30"/>
    <attnum name="mu" min="0.05" max="1.6" val="1.6"/>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="50" max="200" val="80"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ypos" unit="m" val="0.85"/>
    <attnum name="rim diameter" unit="in" val="18.0" min="16"/>
    <attnum name="tire width" unit="mm" val="335"/>
    <attnum name="tire height-width ratio" val=".30"/>
    <attnum name="mu" min="0.05" max="1.6" val="1.6"/>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="50" max="200" val="80"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="2000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="1"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="2000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="1.5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="80"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="80"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="80"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="80"/>
  </section>

  <section name="Front Right Brake">
    <attnum name="disk diameter" unit="mm" val="380"/>
  </section>

  <section name="Front Left Brake">
    <attnum name="disk diameter" unit="mm" val="380"/>
  </section>

  <section name="Rear Right Brake">
    <attnum name="disk diameter" unit="mm" val="280"/>
  </section>

  <section name="Rear Left Brake">
    <attnum name="disk diameter" unit="mm" val="280"/>
  </section>

</params>


