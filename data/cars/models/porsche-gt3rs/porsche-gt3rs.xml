<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Porsche GT3 RS" type="template">

<!--  3D model by Eric Espie -->

  <section name="Driver">
    <!-- Position of the driver -->
    <attnum name="xpos" val="0.5" unit="m"/>
    <attnum name="ypos" val="0.0" unit="m"/>
    <attnum name="zpos" val="1.0" unit="m"/>
  </section>

  <section name="Graphic Objects">
    <attstr name="env" val="porsche-gt3rs.ac"/>
    <attstr name="wheel texture" val="cars/porsche-gt3rs/tex-wheel.rgb"/>
    <attstr name="tachometer texture" val="rpm8500.rgb"/>
    <attnum name="tachometer min value" val="0" unit="rpm"/>
    <attnum name="tachometer max value" val="10000" unit="rpm"/>
    <attstr name="speedometer texture" val="speed360.rgb"/>
    <attnum name="speedometer min value" val="0" unit="km/h"/>
    <attnum name="speedometer max value" val="360" unit="km/h"/>

    <section name="Ranges">
      <section name="1">
	<attnum name="threshold" val="0"/>
	<attstr name="car" val="porsche-gt3rs.ac"/>
      </section>
    </section>
  </section>

  <section name="Car">
    <attstr name="category" val="Track-RWD-GrA"/>

    <attnum name="body length" unit="m" val="4.43"/>
    <attnum name="body width" unit="m" val="1.8"/>
    <attnum name="body height" unit="m" val="1.15"/>
    <!-- collision bounding box -->
    <attnum name="overall length" unit="m" val="4.43"/>
    <attnum name="overall width"  unit="m" val="1.8"/>
    <attnum name="mass" unit="kg" val="1050.0"/>
    <attnum name="GC height" unit="m" val="0.20"/>
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" val="0.45"/>
    <attnum name="front right-left weight repartition" min="0.3" max="0.7" val="0.5"/>
    <attnum name="rear right-left weight repartition" min="0.3" max="0.7" val="0.5"/>
    <!-- used for inertia, indicates the good mass centering (lower values) -->
    <attnum name="mass repartition coefficient" val="0.5"/>
    <attnum name="fuel tank" unit="l" val="100.0"/>
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="85.0"/>
  </section>

  <section name="Exhaust">
    <!-- for flames -->
    <attnum name="power" val="1.5"/>
    <section name="1">
      <attnum name="xpos" val="-2.12"/>
      <attnum name="ypos" val="-0.58"/>
      <attnum name="zpos" val="0.23"/>
    </section>
    <section name="2">
      <attnum name="xpos" val="-2.12"/>
      <attnum name="ypos" val="0.58"/>
      <attnum name="zpos" val="0.23"/>
    </section>
  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" val="0.30"/>
    <attnum name="front area" unit="m2" val="1.5"/>
    <attnum name="front Clift" min="0.0" max="1.0" val="0.9"/>
    <attnum name="rear Clift"  min="0.0" max="1.0" val="0.5"/>
  </section>

  <section name="Front Wing">
    <attnum name="area" unit="m2" val="0.2"/>
    <attnum name="angle" unit="deg" min="0" max="20" val="2"/>
    <attnum name="xpos" unit="m" val="2.15"/>
    <attnum name="zpos" unit="m" val="0.1"/>
  </section>

  <section name="Rear Wing">
    <attnum name="area"  unit="m2" val="1.5"/>
    <attnum name="angle" unit="deg" min="0" max="30" val="15"/>
    <attnum name="xpos" unit="m" val="-1.95"/>
    <attnum name="zpos" unit="m" val="0.95"/>
  </section>

  <!-- Same engine for every one -->
  <section name="Engine">
    <attnum name="revs limiter" unit="rpm"   val="8500"/>
    <attnum name="revs maxi"    unit="rpm"   val="10000"/>
    <attnum name="tickover"     unit="rpm"   val="1000"/>
    <attnum name="fuel cons factor"          val="1.2"/>
    <section name="data points">
      <section name="1">
	<attnum name="rpm" unit="rpm" val="0"/>
	<attnum name="Tq" min="0" max="400" unit="N.m" val="200"/>
      </section>
      <section name="2">
	<attnum name="rpm" unit="rpm" val="500"/>
	<attnum name="Tq" min="0" max="500" unit="N.m" val="300"/>
      </section>
      <section name="3">
	<attnum name="rpm" unit="rpm" val="1000"/>
	<attnum name="Tq" min="0" max="600" unit="N.m" val="500"/>
      </section>
      <section name="4">
	<attnum name="rpm" unit="rpm" val="1500"/>
	<attnum name="Tq" min="0" max="650" unit="N.m" val="510"/>
      </section>
      <section name="5">
	<attnum name="rpm" unit="rpm" val="2000"/>
	<attnum name="Tq" min="0" max="700" unit="N.m" val="520"/>
      </section>
      <section name="6">
	<attnum name="rpm" unit="rpm" val="2500"/>
	<attnum name="Tq" min="0" max="750" unit="N.m" val="530"/>
      </section>
      <section name="7">
	<attnum name="rpm" unit="rpm" val="3000"/>
	<attnum name="Tq" min="0" max="750" unit="N.m" val="550"/>
      </section>
      <section name="8">
	<attnum name="rpm" unit="rpm" val="3500"/>
	<attnum name="Tq" min="0" max="750" unit="N.m" val="560"/>
      </section>
      <section name="9">
	<attnum name="rpm" unit="rpm" val="4000"/>
	<attnum name="Tq" min="0" max="750" unit="N.m" val="580"/>
      </section>
      <section name="10">
	<attnum name="rpm" unit="rpm" val="4500"/>
	<attnum name="Tq" min="0" max="750" unit="N.m" val="600"/>
      </section>
      <section name="11">
	<attnum name="rpm" unit="rpm" val="5000"/>
	<attnum name="Tq" min="0" max="750" unit="N.m" val="620"/>
      </section>
      <section name="12">
	<attnum name="rpm" unit="rpm" val="5500"/>
	<attnum name="Tq" min="0" max="750" unit="N.m" val="640"/>
      </section>
      <section name="13">
	<attnum name="rpm" unit="rpm" val="6000"/>
	<attnum name="Tq" min="0" max="750" unit="N.m" val="650"/>
      </section>
      <section name="14">
	<attnum name="rpm" unit="rpm" val="6500"/>
	<attnum name="Tq" min="0" max="725" unit="N.m" val="625"/>
      </section>
      <section name="15">
	<attnum name="rpm" unit="rpm" val="7000"/>
	<attnum name="Tq" min="0" max="700" unit="N.m" val="675"/>
      </section>
      <section name="16">
	<attnum name="rpm" unit="rpm" val="7500"/>
	<attnum name="Tq" min="0" max="650" unit="N.m" val="625"/>
      </section>
      <section name="17">
	<attnum name="rpm" unit="rpm" val="8000"/>
	<attnum name="Tq" min="0" max="600" unit="N.m" val="550"/>
      </section>
      <section name="18">
	<attnum name="rpm" unit="rpm" val="8500"/>
	<attnum name="Tq" min="0" max="550" unit="N.m" val="475"/>
      </section>
      <section name="19">
	<attnum name="rpm" unit="rpm" val="9000"/>
	<attnum name="Tq" min="0" max="500" unit="N.m" val="400"/>
      </section>
      <section name="20">
	<attnum name="rpm" unit="rpm" val="9500"/>
	<attnum name="Tq" min="0" max="450" unit="N.m" val="375"/>
      </section>
      <section name="21">
	<attnum name="rpm" unit="rpm" val="10000"/>
	<attnum name="Tq" min="0" max="400" unit="N.m" val="350"/>
      </section>
    </section>
  </section>

  <section name="Clutch">
    <!-- pressure plate -->
    <attnum name="inertia" unit="kg.m2" val="0.1150"/>
  </section>

  <section name="Gearbox">
    <attnum name="shift time" unit="s" val="0.15"/>
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"/>
	<attnum name="inertia" val="0.0037"/>
	<attnum name="efficiency" val="0.957"/>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="2.66"/>
	<attnum name="inertia" val="0.003"/>
	<attnum name="efficiency" val="0.955"/>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="1.78"/>
	<attnum name="inertia" val="0.0037"/>
	<attnum name="efficiency" val="0.957"/>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.3"/>
	<attnum name="inertia" val="0.0048"/>
	<attnum name="efficiency" val="0.950"/>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.00"/>
	<attnum name="inertia" val="0.0064"/>
	<attnum name="efficiency" val="0.983"/>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val=".74"/>
	<attnum name="inertia" val="0.0107"/>
	<attnum name="efficiency" val="0.948"/>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.50"/>
	<attnum name="inertia" val="0.0150"/>
	<attnum name="efficiency" val="0.940"/>
      </section>
    </section>
  </section>

  <section name="Drivetrain">
    <!-- allowed values RWD, FWD, 4WD -->
    <attstr name="type"                 val="RWD"/>
    <!-- drive shaft inertia -->
    <attnum name="inertia" unit="kg.m2" val="0.0091"/>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="1" max="45" val="45"/>
    <attnum name="max steer speed" unit="deg/s" min="1" max="360" val="120"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.55"/>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="50000"/>
  </section>

  <section name="Front Axle">
    <attnum name="xpos" min="0.5" max="2.5" val="1.23"/>
    <attnum name="inertia" unit="kg.m2"        val="0.0056"/>
    <attnum name="roll center height" unit="m" min="0" max="0.5" val="0.012"/>
  </section>

  <section name="Rear Axle">
    <attnum name="xpos" min="-2.5" max="-0.5" val="-1.15"/>
    <attnum name="inertia" unit="kg.m2"        val="0.0080"/>
    <attnum name="roll center height" unit="m" min="0" max="0.5" val="0.04"/>
  </section>

  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="NONE" val="NONE"/>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"/>
    <attnum name="inertia" unit="kg.m2" val="0.0488"/>
    <attnum name="ratio" min="0" max="10" val="3.07"/>
    <attnum name="efficiency"           val="0.9625"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ypos" unit="m" val="-0.82"/>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"/>
    <attnum name="tire width" unit="mm" min="135" max="285" val="225"/>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".40"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="100" max="300" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
    <!-- Adherence -->
    <attnum name="stiffness" val="25.0"/>
    <attnum name="dynamic friction" unit="%" val="80"/>
    <attnum name="rolling resistance" val="0.02"/>
    <attnum name="mu" min="0.05" max="1.5" val="1.5"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ypos" unit="m" val="0.82"/>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"/>
    <attnum name="tire width" unit="mm" min="135" max="285" val="225"/>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".40"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="100" max="300" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
    <!-- Adherence -->
    <attnum name="stiffness" val="25.0"/>
    <attnum name="dynamic friction" unit="%" val="80"/>
    <attnum name="rolling resistance" val="0.02"/>
    <attnum name="mu" min="0.05" max="1.5" val="1.5"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ypos" unit="m" val="-0.81"/>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"/>
    <attnum name="tire width" unit="mm" min="135" max="335" val="285"/>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".35"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="100" max="300" val="100"/>
    <attnum name="toe" unit="deg" min="0" max="0" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
    <!-- Adherence -->
    <attnum name="stiffness" val="25.0"/>
    <attnum name="dynamic friction" unit="%" val="80"/>
    <attnum name="rolling resistance" val="0.02"/>
    <attnum name="mu" min="0.05" max="1.5" val="1.5"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ypos" unit="m" val="0.81"/>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"/>
    <attnum name="tire width" unit="mm" min="135" max="335" val="285"/>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".35"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="100" max="300" val="100"/>
    <attnum name="toe" unit="deg" min="0" max="0" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
    <!-- Adherence -->
    <attnum name="stiffness" val="25.0"/>
    <attnum name="dynamic friction" unit="%" val="80"/>
    <attnum name="rolling resistance" val="0.02"/>
    <attnum name="mu" min="0.05" max="1.5" val="1.5"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2.5"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2.5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"/>
    <attnum name="suspension course" unit="m" min="0" max="0.25" val="0.25"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.2"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"/>
    <attnum name="suspension course" unit="m" min="0" max="0.25" val="0.25"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.2"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"/>
    <attnum name="suspension course" unit="m" min="0" max="0.25" val="0.25"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"/>
    <attnum name="suspension course" unit="m" min="0" max="0.25" val="0.25"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"/>
  </section>

  <section name="Front Right Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="330"/>
    <attnum name="piston area"   unit="cm2"   val="50"/>
    <attnum name="mu"                         val="0.45"/>
    <attnum name="inertia"       unit="kg.m2" val="0.1241"/>
  </section>

  <section name="Front Left Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="330"/>
    <attnum name="piston area"   unit="cm2"   val="50"/>
    <attnum name="mu"                         val="0.45"/>
    <attnum name="inertia"       unit="kg.m2" val="0.1241"/>
  </section>

  <section name="Rear Right Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="330"/>
    <attnum name="piston area"   unit="cm2"   val="25"/>
    <attnum name="mu"                         val="0.45"/>
    <attnum name="inertia"       unit="kg.m2" val="0.0714"/>
  </section>

  <section name="Rear Left Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="330"/>
    <attnum name="piston area"   unit="cm2"   val="25"/>
    <attnum name="mu"                         val="0.45"/>
    <attnum name="inertia"       unit="kg.m2" val="0.0714"/>
  </section>

</params>



