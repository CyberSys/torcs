<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Alfa Romeo 156" type="template">

  <section name="Graphic Objects">
    <attstr name="wheel texture" val="tex-wheel.rgb"/>
    <attstr name="shadow texture" val="shadow.rgb"/>
    <attstr name="tachometer texture" val="rpm8500.rgb"/>
    <attnum name="tachometer min value" unit="rpm" val="0"/>
    <attnum name="tachometer max value" unit="rpm" val="10000"/>
    <attstr name="speedometer texture" val="speed300.rgb"/>
    <attnum name="speedometer min value" unit="km/h" val="0"/>
    <attnum name="speedometer max value" unit="km/h" val="300"/>

    <section name="Ranges">
      <section name="1">
	<attnum name="threshold" val="10"/>
	<attstr name="car" val="alpha-156-rk.acc"/>
      </section>
      <section name="2">
	<attnum name="threshold" val="0"/>
	<attstr name="car" val="alpha-156-rk-lod.acc"/>
      </section>
    </section>
  </section>

  <section name="Driver">
    <!-- Position of the driver -->
    <attnum name="xpos" unit="m" val="0.60"/>
    <attnum name="ypos" unit="m" val="0.0"/>
    <attnum name="zpos" unit="m" val="1.20"/>
  </section>

  <section name="Car">
    <attstr name="category" val="Track-FWD-GrB"/>
    <attstr name="author" val="Ryuji Kainoh"/>
    <attnum name="body length" unit="m" val="4.43"/>
    <attnum name="body width" unit="m" val="2.0"/>
    <attnum name="body height" unit="m" val="1.38"/>
    <!-- collision bounding box -->
    <attnum name="overall length" unit="m" val="4.43"/>
    <attnum name="overall width" unit="m" val="2.0"/>
    <attnum name="mass" unit="kg" val="1140.0"/>
    <attnum name="GC height" unit="m" val="0.25"/>
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" val="0.55"/>
    <attnum name="front right-left weight repartition" val="0.5"/>
    <attnum name="rear right-left weight repartition" val="0.5"/>
    <!-- used for inertia, indicates the good mass centering -->
    <attnum name="mass repartition coefficient" val="0.8"/>
    <attnum name="fuel tank" unit="l" val="80.0"/>
    <attnum name="initial fuel" min="1.0" max="80.0" unit="l" val="5.0"/>
  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" val="0.22"/>
    <attnum name="front area" unit="m2" val="2.0"/>
    <attnum name="front Clift" val="0.5"/>
    <attnum name="rear Clift" val="0.3"/>
  </section>

  <section name="Front Wing">
    <attnum name="area" unit="m2" val="0.3"/>
    <attnum name="angle" min="0" max="30" unit="deg" val="0"/>
    <attnum name="xpos" unit="m" val="2.15"/>
    <attnum name="zpos" unit="m" val="0.1"/>
  </section>

  <section name="Rear Wing">
    <attnum name="area" min="0" max="1.0" unit="m2" val="1.0"/>
    <attnum name="angle" min="0" max="40" unit="deg" val="0"/>
    <attnum name="xpos" unit="m" val="-2.08"/>
    <attnum name="zpos" unit="m" val="1.24"/>
  </section>

  <section name="Engine">
    <attnum name="inertia" unit="kg.m2" val="0.1"/>
    <attnum name="revs limiter" unit="rpm" val="8500"/>
    <attnum name="revs maxi" unit="rpm" val="8500"/>
    <attnum name="tickover" unit="rpm" val="1000"/>
    <attnum name="fuel cons factor" val="1.0"/>
    <section name="data points">
      <section name="1">
	<attnum name="rpm" unit="rpm" val="0"/>
	<attnum name="Tq" min="0" max="300" unit="N.m" val="50"/>
