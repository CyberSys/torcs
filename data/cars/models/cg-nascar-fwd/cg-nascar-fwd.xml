<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="NASCAR FWD" type="template">

  <section name="Graphic Objects">
    <attstr name="env" val="cg-nascar-fwd.ac"></attstr>
    <attstr name="wheel texture" val="tex-wheel.rgb"></attstr>

    <section name="Ranges">
      <section name="1">
	<attnum name="distance" val="500" unit="km"></attnum>
	<attstr name="car" val="cg-nascar-fwd.ac"></attstr>
      </section>
    </section>
  </section>

  <section name="Driver">
    <!-- Position of the driver -->
    <attnum name="xpos" val="0.45" unit="m"></attnum>
    <attnum name="ypos" val="0.0" unit="m"></attnum>
    <attnum name="zpos" val="1.10" unit="m"></attnum>
  </section>

  <section name="Car">
    <attstr name="category" val="Track-FWD-GrA"></attstr>    
    <attnum name="body length" unit="m" val="5.0"></attnum>
    <attnum name="body width"  unit="m" val="2.0"></attnum>
    <attnum name="body height" unit="m" val="1.2"></attnum>
    <!-- collision bounding box -->
    <attnum name="overall length" unit="m" val="5.0"></attnum>
    <attnum name="overall width"  unit="m" val="2.0"></attnum>

    <attnum name="mass" unit="kg" val="1000.0"></attnum>
    <attnum name="GC height" unit="m" val="0.20"></attnum>
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" val="0.6"></attnum>
    <attnum name="front right-left weight repartition" min="0.4" max="0.6" val="0.5"></attnum>
    <attnum name="rear right-left weight repartition" min="0.4" max="0.6" val="0.5"></attnum>
    <!-- used for inertia, smaller values indicate better mass centering -->
    <attnum name="mass repartition coefficient" val="0.8"></attnum>
    <attnum name="fuel tank" unit="l" val="100.0"></attnum>
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="100.0"></attnum>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="1" max="45" val="45"></attnum>
  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" val="0.38"></attnum>
    <attnum name="front area" unit="m2" val="1.7"></attnum>
    <attnum name="front Clift" min="0.0" max="1.0" val="0.6"></attnum>
    <attnum name="rear Clift"  min="0.0" max="1.0" val="0.4"></attnum>
  </section>

  <section name="Front Wing">
    <attnum name="area" unit="m2" val="0"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="area"  unit="m2" val="1.0"></attnum>
    <attnum name="angle" unit="deg" min="0" max="30" val="30"></attnum>
    <attnum name="xpos" unit="m" val="-2.3"></attnum>
    <attnum name="zpos" unit="m" val=".6"></attnum>
  </section>

  <!-- Same engine for every one -->

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"></attnum>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="3.82"></attnum>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="2.15"></attnum>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.56"></attnum>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.21"></attnum>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.97"></attnum>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.75"></attnum>
      </section>
    </section>
  </section>

  <section name="Drivetrain">
    <attstr name="type"  val="FWD"></attstr>
  </section>

  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="FREE"></attstr>
    <attnum name="ratio" min="0" max="5" val="3.444"></attnum>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.65"></attnum>
    <attnum name="max pressure" unit="kPa" min="1000" max="300000" val="11000"></attnum>
  </section>

  <section name="Front Axle">
    <attnum name="xpos" unit="m" val="1.60"></attnum>
    <attnum name="roll center height" unit="m" min="0.012" max="0.5" val="0.012"></attnum>
  </section>

  <section name="Rear Axle">
    <attnum name="xpos" unit="m" val="-1.36"></attnum>
    <attnum name="roll center height" unit="m" min="0.04" max="0.5" val="0.04"></attnum>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ypos" unit="m" val="-0.81"></attnum>
    <attnum name="rim diameter" unit="in" val="14.5"></attnum>
    <attnum name="tire width" unit="mm" val="335"></attnum>
    <attnum name="tire height-width ratio" val=".45"></attnum>
    <attnum name="mu"  val="1.6"></attnum>
    <attnum name="stiffness" min="5.0" max="50.0" val="10.0"></attnum>
    <attnum name="dynamic friction" unit="%" val="90"></attnum>
    <attnum name="rolling resistance" val="0.015"></attnum>
    <attnum name="ride height" unit="mm" min="100" max="200" val="100"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ypos" unit="m" val="0.81"></attnum>
    <attnum name="rim diameter" unit="in" val="14.5"></attnum>
    <attnum name="tire width" unit="mm" val="335"></attnum>
    <attnum name="tire height-width ratio" val=".45"></attnum>
    <attnum name="mu"  val="1.6"></attnum>
    <attnum name="stiffness" min="5.0" max="50.0" val="10.0"></attnum>
    <attnum name="dynamic friction" unit="%" val="90"></attnum>
    <attnum name="rolling resistance" val="0.015"></attnum>
    <attnum name="ride height" unit="mm" min="100" max="200" val="100"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ypos" unit="m" val="-0.81"></attnum>
    <attnum name="rim diameter" unit="in" val="14.5"></attnum>
    <attnum name="tire width" unit="mm" val="335"></attnum>
    <attnum name="tire height-width ratio" val=".45"></attnum>
    <attnum name="mu" min="0.05" max="1.6" val="1.6"></attnum>
    <attnum name="stiffness" min="5.0" max="50.0" val="10.0"></attnum>
    <attnum name="rolling resistance" val="0.015"></attnum>
    <attnum name="dynamic friction" unit="%" val="90"></attnum>
    <attnum name="ride height" unit="mm" min="100" max="200" val="100"></attnum>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ypos" unit="m" val="0.81"></attnum>
    <attnum name="rim diameter" unit="in" val="14.5"></attnum>
    <attnum name="tire width" unit="mm" val="335"></attnum>
    <attnum name="tire height-width ratio" val=".45"></attnum>
    <attnum name="mu" val="1.6"></attnum>
    <attnum name="stiffness" min="5.0" max="50.0" val="10.0"></attnum>
    <attnum name="rolling resistance" val="0.015"></attnum>
    <attnum name="dynamic friction" unit="%" val="90"></attnum>
    <attnum name="ride height" unit="mm" min="100" max="200" val="100"></attnum>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" val="0.2"></attnum>
    <attnum name="bellcrank" val="2.0"></attnum>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" val="0.2"></attnum>
    <attnum name="bellcrank" val="2.0"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Front Right Brake">
    <attnum name="disk diameter" unit="mm" val="380"></attnum>
  </section>

  <section name="Front Left Brake">
    <attnum name="disk diameter" unit="mm" val="380"></attnum>
  </section>

  <section name="Rear Right Brake">
    <attnum name="disk diameter" unit="mm" val="280"></attnum>
  </section>

  <section name="Rear Left Brake">
    <attnum name="disk diameter" unit="mm" val="280"></attnum>
  </section>

</params>


