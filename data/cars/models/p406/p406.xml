<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Peugeot 406" type="template">

    
  <section name="Graphic Objects">
    <attstr name="env" val="p406.ac"/>
    <attstr name="bonnet" val="p406.ac"/>
    <attstr name="wheel texture" val="tex-wheel.rgb"/>
    <attstr name="tachometer texture" val="rpm7000.rgb"/>
    <attnum name="tachometer min value" val="0" unit="rpm"/>
    <attnum name="tachometer max value" val="8000" unit="rpm"/>
    <attstr name="speedometer texture" val="speed300.rgb"/>
    <attnum name="speedometer min value" val="0" unit="km/h"/>
    <attnum name="speedometer max value" val="300" unit="km/h"/>

    <section name="Ranges">
      <section name="1">
	<attnum name="distance" val="500"/>
	<attstr name="car" val="p406.ac"/>
      </section>
    </section>
  </section>

  <section name="Driver">
    <!-- Position of the driver -->
    <attnum name="xpos" val="0.8" unit="m"/>
    <attnum name="ypos" val="0.0" unit="m"/>
    <attnum name="zpos" val="1.1" unit="m"/>
  </section>

  <section name="Car">
    <attstr name="category" val="Track-FWD-GrB"/>    
    <attnum name="body length" unit="m" min="4.5" max="5.0" val="4.60"/>
    <attnum name="body width" unit="m" min="0.8" max="2.0" val="1.76"/>
    <attnum name="body height" unit="m" min="1.1" max="3.0" val="1.16"/>
    <!-- collision bounding box -->
    <attnum name="overall length" unit="m" min="4.5" max="5.0" val="4.6"/>
    <attnum name="overall width"  unit="m" min="1.2" max="2.0" val="1.76"/>
    <attnum name="mass" unit="kg" min="600.0" max="2500" val="1000.0"/>
    <attnum name="GC height" unit="m" min="0.25" max="1.0" val="0.20"/>
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" min="0.4" max="0.7" val="0.58"/>
    <attnum name="front right-left weight repartition" min="0.3" max="0.7" val="0.5"/>
    <attnum name="rear right-left weight repartition" min="0.3" max="0.7" val="0.5"/>
    <!-- used for inertia, indicates the good mass centering -->
    <attnum name="mass repartition coefficient" min="0.2" max="1.5" val="0.9"/>
    <attnum name="fuel tank" unit="l" min="30.0" max="100.0" val="100.0"/>
    <attnum name="initial fuel" val="100.0" unit="l" min="1.0" max="100.0"/>
  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" min="0.20" max="2.0" val="0.4"/>
    <attnum name="front area" unit="m2" min="1.0" max="5.0" val="2.085"/>
    <attnum name="front Clift" val="0"/>
    <attnum name="rear Clift"  val="0"/>
  </section>

  <section name="Front Wing">
    <attnum name="area" unit="m2" val="0"/>
  </section>

  <section name="Rear Wing">
    <attnum name="area"  unit="m2" min="0" max="1.0" val="0.4"/>
    <attnum name="angle" unit="deg" min="0" max="30" val="30"/>
    <attnum name="xpos" unit="m" min="-2.5" max="-1.0" val="-2.3"/>
    <attnum name="zpos" unit="m" min="0.1" max="1.5" val=".6"/>
  </section>

  <!-- Same engine for every one -->


  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"/>
	<attnum name="inertia" val="0.0037"/>
	<attnum name="efficiency" val="0.957"/>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="3.82"/>
	<attnum name="inertia" val="0.003"/>
	<attnum name="efficiency" val="0.955"/>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="2.15"/>
	<attnum name="inertia" val="0.0037"/>
	<attnum name="efficiency" val="0.957"/>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.56"/>
	<attnum name="inertia" val="0.0048"/>
	<attnum name="efficiency" val="0.950"/>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.21"/>
	<attnum name="inertia" val="0.0064"/>
	<attnum name="efficiency" val="0.983"/>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.97"/>
	<attnum name="inertia" val="0.0107"/>
	<attnum name="efficiency" val="0.948"/>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.75"/>
	<attnum name="inertia" val="0.0150"/>
	<attnum name="efficiency" val="0.940"/>
      </section>
    </section>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="10" max="45" val="45"/>
    <attnum name="max steer speed" unit="deg/s" min="1" max="360" val="120"/>
  </section>


  <section name="Drivetrain">
    <!-- allowed values RWD, FWD, 4WD -->
    <attstr name="type"                 val="FWD"/>
    <!-- drive shaft inertia -->
    <attnum name="inertia" unit="kg.m2" val="0.0091"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.70"/>
    <attnum name="max pressure" unit="kPa" min="1000" max="500000" val="30000"/>
  </section>

  <section name="Front Axle">
    <attnum name="xpos" min="0.5" max="2.5" val="1.34"/>
    <attnum name="inertia" unit="kg.m2"        val="0.0056"/>
    <attnum name="roll center height" unit="m" min="0" max="1.0" val="0.4"/>
  </section>

  <section name="Rear Axle">
    <attnum name="xpos" min="-2.5" max="-0.5" val="-1.37"/>
    <attnum name="inertia" unit="kg.m2"        val="0.0080"/>
    <attnum name="roll center height" unit="m" min="0" max="1.0" val="0.5"/>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" val="NONE"/>
  </section>

  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP,VISCOUS COUPLER" val="SPOOL"/>
    <attnum name="inertia" unit="kg.m2" val="0.0488"/>
    <attnum name="ratio" min="0" max="10" val="3.444"/>
    <attnum name="efficiency"           val="0.9625"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ypos" unit="m" val="-0.75"/>
    <attnum name="rim diameter" min="15" max="17" val="15.0" unit="in"/>
    <attnum name="tire width" unit="mm" val="245"/>
    <attnum name="tire height-width ratio" val=".40"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <attnum name="mu" min="0.05" max="1.4" val="1.4"/>
    <attnum name="rolling resistance" val="0.0105"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="50" max="1000" val="180"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ypos" unit="m" val="0.75"/>
    <attnum name="rim diameter" min="15" max="17" val="15.0" unit="in"/>
    <attnum name="tire width" unit="mm" val="245"/>
    <attnum name="tire height-width ratio" val=".40"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <attnum name="mu" min="0.05" max="1.4" val="1.4"/>
    <attnum name="rolling resistance" val="0.0105"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="ride height" unit="mm" min="50" max="1000" val="180"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ypos" unit="m" val="-0.73"/>
    <attnum name="rim diameter" min="15" max="17" val="15.0" unit="in"/>
    <attnum name="tire width" unit="mm" val="245"/>
    <attnum name="tire height-width ratio" val=".40"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <attnum name="mu" min="0.05" max="1.4" val="1.4"/>
    <attnum name="rolling resistance" val="0.0105"/>
    <attnum name="ride height" unit="mm" min="50" max="1000" val="184"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ypos" unit="m" val="0.73"/>
    <attnum name="rim diameter" min="15" max="17" val="15.0" unit="in"/>
    <attnum name="tire width" unit="mm" val="245"/>
    <attnum name="tire height-width ratio" val=".40"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <attnum name="mu" min="0.05" max="1.4" val="1.4"/>
    <attnum name="rolling resistance" val="0.0105"/>
    <attnum name="ride height" unit="mm" min="50" max="1000" val="184"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="1000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2.5"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="2000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2.5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.5" val="0.2"/>
    <attnum name="bellcrank" min="0.1" max="5" val="2"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.5" val="0.2"/>
    <attnum name="bellcrank" min="0.1" max="5" val="2"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.5" val="0.2"/>
    <attnum name="bellcrank" min="0.1" max="5" val="2"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.5" val="0.2"/>
    <attnum name="bellcrank" min="0.1" max="5" val="2"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"/>
  </section>

  <section name="Front Right Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="280"/>
    <attnum name="piston area"   unit="cm2"   val="25"/>
    <attnum name="mu"                         val="0.30"/>
    <attnum name="inertia"       unit="kg.m2" val="0.1441"/>
  </section>

  <section name="Front Left Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="280"/>
    <attnum name="piston area"   unit="cm2"   val="25"/>
    <attnum name="mu"                         val="0.30"/>
    <attnum name="inertia"       unit="kg.m2" val="0.1441"/>
  </section>

  <section name="Rear Right Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="120"/>
    <attnum name="piston area"   unit="cm2"   val="8"/>
    <attnum name="mu"                         val="0.30"/>
    <attnum name="inertia"       unit="kg.m2" val="0.1014"/>
  </section>

  <section name="Rear Left Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="120"/>
    <attnum name="piston area"   unit="cm2"   val="8"/>
    <attnum name="mu"                         val="0.30"/>
    <attnum name="inertia"       unit="kg.m2" val="0.1014"/>
  </section>

</params>


