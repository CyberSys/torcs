<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Alfa Romeo 155 DTM" type="template">

<!-- The car 3D model is used with the authorization of ViPeR (viper@mttestdriver.com) -->

  <section name="Driver">
    <!-- Position of the driver -->
    <attnum name="xpos" val="0.50" unit="m"></attnum>
    <attnum name="ypos" val="0.0" unit="m"></attnum>
    <attnum name="zpos" val="0.95" unit="m"></attnum>
  </section>

  <section name="Graphic Objects">
    <attstr name="env" val="155-DTM.ac"></attstr>
    <attstr name="wheel texture" val="cars/155-DTM/tex-wheel.rgb"></attstr>
    <attstr name="tachometer texture" val="rpm8500.rgb"></attstr>
    <attnum name="tachometer min value" val="0" unit="rpm"></attnum>
    <attnum name="tachometer max value" val="10000" unit="rpm"></attnum>
    <attstr name="speedometer texture" val="speed300.rgb"></attstr>
    <attnum name="speedometer min value" val="0" unit="km/h"></attnum>
    <attnum name="speedometer max value" val="300" unit="km/h"></attnum>

    <section name="Ranges">
      <section name="1">
	<attnum name="distance" unit="km" val="600"></attnum>
	<attstr name="car" val="155-DTM.ac"></attstr>
      </section>
    </section>
  </section>

  <section name="Car">
    <attstr name="category" val="torcs-a-b"></attstr>    
    <attnum name="body length" unit="m" min="4.5" max="5.0" val="4.8"></attnum>
    <attnum name="body width" unit="m" min="0.8" max="2.1" val="1.9"></attnum>
    <attnum name="body height" unit="m" min="1.1" max="1.7" val="1.2"></attnum>
    <!-- collision bounding box -->
    <attnum name="overall length" unit="m" min="4.5" max="5.0" val="4.8"></attnum>
    <attnum name="overall width"  unit="m" min="1.2" max="2.1" val="1.9"></attnum>
    <attnum name="mass" unit="kg" min="1100.0" max="2500" val="1100.0"></attnum>
    <attnum name="GC height" unit="m" min="0.15" max="0.5" val="0.20"></attnum>
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" min="0.4" max="0.6" val="0.5"></attnum>
    <attnum name="front right-left weight repartition" min="0.4" max="0.6" val="0.5"></attnum>
    <attnum name="rear right-left weight repartition" min="0.4" max="0.6" val="0.5"></attnum>
    <!-- used for inertia, indicates the good mass centering -->
    <attnum name="mass repartition coefficient" min="0.2" max="1.0" val="0.9"></attnum>
    <attnum name="fuel tank" unit="l" min="30.0" max="100.0" val="100.0"></attnum>
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="30.0"></attnum>
  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" val="0.24"></attnum>
    <attnum name="front area" unit="m2" val="1.6"></attnum>
    <attnum name="Clift" val="1.0"></attnum>
  </section>

  <section name="Front Wing">
    <attnum name="area" unit="m2" val="0.25"></attnum>
    <attnum name="angle" unit="deg" min="0" max="30" val="20"></attnum>
    <attnum name="xpos" unit="m" val="2.22"></attnum>
    <attnum name="zpos" unit="m" val="0.1"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="area"  unit="m2" val="0.8"></attnum>
    <attnum name="angle" unit="deg" min="0" max="30" val="20"></attnum>
    <attnum name="xpos" unit="m" val="-1.94"></attnum>
    <attnum name="zpos" unit="m" val="1.03"></attnum>
  </section>

  <!-- Engine Max curve -->
  <section name="Engine">
    <attnum name="revs limiter"  unit="rpm" val="8500"></attnum>
    <attnum name="revs maxi"  unit="rpm" val="10000"></attnum>
    <attnum name="tickover" min="500" max="5000" unit="rpm" val="2000"></attnum>
    <section name="data points">
      <section name="1">
	<attnum name="rpm" unit="rpm" val="0"></attnum>
	<attnum name="Tq" min="0" max="280" unit="N.m" val="280"></attnum>
      </section>
      <section name="2">
	<attnum name="rpm" unit="rpm" val="500"></attnum>
	<attnum name="Tq" min="0" max="300" unit="N.m" val="300"></attnum>
      </section>
      <section name="3">
	<attnum name="rpm" unit="rpm" val="1000"></attnum>
	<attnum name="Tq" min="0" max="350" unit="N.m" val="350"></attnum>
      </section>
      <section name="4">
	<attnum name="rpm" unit="rpm" val="1500"></attnum>
	<attnum name="Tq" min="0" max="390" unit="N.m" val="390"></attnum>
      </section>
      <section name="5">
	<attnum name="rpm" unit="rpm" val="2000"></attnum>
	<attnum name="Tq" min="0" max="430" unit="N.m" val="430"></attnum>
      </section>
      <section name="6">
	<attnum name="rpm" unit="rpm" val="2500"></attnum>
	<attnum name="Tq" min="0" max="430" unit="N.m" val="430"></attnum>
      </section>
      <section name="7">
	<attnum name="rpm" unit="rpm" val="3000"></attnum>
	<attnum name="Tq" min="0" max="450" unit="N.m" val="450"></attnum>
      </section>
      <section name="8">
	<attnum name="rpm" unit="rpm" val="3500"></attnum>
	<attnum name="Tq" min="0" max="450" unit="N.m" val="450"></attnum>
      </section>
      <section name="9">
	<attnum name="rpm" unit="rpm" val="4000"></attnum>
	<attnum name="Tq" min="0" max="460" unit="N.m" val="460"></attnum>
      </section>
      <section name="10">
	<attnum name="rpm" unit="rpm" val="4500"></attnum>
	<attnum name="Tq" min="0" max="460" unit="N.m" val="460"></attnum>
      </section>
      <section name="11">
	<attnum name="rpm" unit="rpm" val="5000"></attnum>
	<attnum name="Tq" min="0" max="460" unit="N.m" val="460"></attnum>
      </section>
      <section name="12">
	<attnum name="rpm" unit="rpm" val="5500"></attnum>
	<attnum name="Tq" min="0" max="440" unit="N.m" val="440"></attnum>
      </section>
      <section name="13">
	<attnum name="rpm" unit="rpm" val="6000"></attnum>
	<attnum name="Tq" min="0" max="420" unit="N.m" val="420"></attnum>
      </section>
      <section name="14">
	<attnum name="rpm" unit="rpm" val="6500"></attnum>
	<attnum name="Tq" min="0" max="390" unit="N.m" val="390"></attnum>
      </section>
      <section name="15">
	<attnum name="rpm" unit="rpm" val="7000"></attnum>
	<attnum name="Tq" min="0" max="360" unit="N.m" val="360"></attnum>
      </section>
      <section name="16">
	<attnum name="rpm" unit="rpm" val="7500"></attnum>
	<attnum name="Tq" min="0" max="350" unit="N.m" val="350"></attnum>
      </section>
      <section name="17">
	<attnum name="rpm" unit="rpm" val="8000"></attnum>
	<attnum name="Tq" min="0" max="340" unit="N.m" val="340"></attnum>
      </section>
      <section name="18">
	<attnum name="rpm" unit="rpm" val="8500"></attnum>
	<attnum name="Tq" min="0" max="320" unit="N.m" val="320"></attnum>
      </section>
      <section name="19">
	<attnum name="rpm" unit="rpm" val="9000"></attnum>
	<attnum name="Tq" min="0" max="300" unit="N.m" val="300"></attnum>
      </section>
      <section name="20">
	<attnum name="rpm" unit="rpm" val="9500"></attnum>
	<attnum name="Tq" min="0" max="280" unit="N.m" val="280"></attnum>
      </section>
      <section name="21">
	<attnum name="rpm" unit="rpm" val="10000"></attnum>
	<attnum name="Tq" min="0" max="250" unit="N.m" val="250"></attnum>
      </section>
    </section>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-5" max="0" val="-3.4"></attnum>
	<attnum name="efficiency" val="0.9"></attnum>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="2.533333"></attnum>
	<attnum name="efficiency" val="1.0"></attnum>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="1.650000"></attnum>
	<attnum name="efficiency" val="1.0"></attnum>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.208333"></attnum>
	<attnum name="efficiency" val="1.0"></attnum>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="0.962963"></attnum>
	<attnum name="efficiency" val="1.0"></attnum>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.766667"></attnum>
	<attnum name="efficiency" val="1.0"></attnum>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.65"></attnum>
	<attnum name="efficiency" val="1.0"></attnum>
      </section>
    </section>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="1" max="45" val="45"></attnum>
    <attnum name="max steer speed" unit="deg/s" min="1" max="360" val="120"></attnum>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.55"></attnum>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="11000"></attnum>
  </section>

  <section name="Front Axle">
    <attnum name="xpos" val="1.31"></attnum>
    <attnum name="roll center height" unit="m" val="0.02"></attnum>
  </section>

  <section name="Rear Axle">
    <attnum name="xpos" val="-1.36"></attnum>
    <attnum name="roll center height" unit="m" val="0.08"></attnum>
  </section>

  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="FREE"></attstr>
    <attnum name="ratio" min="0" max="15" val="1.0"></attnum>
    <attnum name="efficiency" min=".5" max="1.0" val="1.0"></attnum>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"></attstr>
    <attnum name="ratio" min="0" max="15" val="1.0"></attnum>
    <attnum name="efficiency" min=".5" max="1.0" val="1.0"></attnum>
  </section>

  <section name="Central Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="NONE,SPOOL,FREE,LIMITED SLIP,VISCOUS COUPLER" val="VISCOUS COUPLER"></attstr>
    <attnum name="min torque bias" val="50" unit="%"></attnum>
    <attnum name="max torque bias" val="60" unit="%"></attnum>
    <attnum name="ratio" min="0" max="10" val="6.0"></attnum>
    <attnum name="efficiency" val="0.9625" min="0.5" max="1.0"></attnum>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ypos" unit="m" val="-0.82"></attnum>
    <attnum name="rim diameter" unit="in" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" val="245"></attnum>
    <attnum name="tire height-width ratio" val=".38"></attnum>
    <attnum name="inertia" unit="kg.m2" val="1.1200"></attnum>
    <attnum name="mu" val="1.6"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"></attnum>
    <attnum name="rolling resistance" val="0.005"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ypos" unit="m" val="0.82"></attnum>
    <attnum name="rim diameter" unit="in" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" val="245"></attnum>
    <attnum name="tire height-width ratio" val=".38"></attnum>
    <attnum name="inertia" unit="kg.m2" val="1.1200"></attnum>
    <attnum name="mu" val="1.6"></attnum>
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"></attnum>
    <attnum name="rolling resistance" val="0.005"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ypos" unit="m" val="-0.83"></attnum>
    <attnum name="rim diameter" unit="in" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" val="245"></attnum>
    <attnum name="tire height-width ratio" val=".38"></attnum>
    <attnum name="inertia" unit="kg.m2" val="1.2200"></attnum>
    <attnum name="mu" val="1.6"></attnum>
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"></attnum>
    <attnum name="rolling resistance" val="0.005"></attnum>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ypos" unit="m" val="0.83"></attnum>
    <attnum name="rim diameter" unit="in" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" val="245"></attnum>
    <attnum name="tire height-width ratio" val=".38"></attnum>
    <attnum name="inertia" unit="kg.m2" val="1.2200"></attnum>
    <attnum name="mu" val="1.6"></attnum>
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"></attnum>
    <attnum name="rolling resistance" val="0.005"></attnum>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
