    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1"></attnum>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"></attnum>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="80"></attnum>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="80"></attnum>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="100"></attnum>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="100"></attnum>
  </section>

  <section name="Front Right Brake">
    <attnum name="disk diameter" min="100" max="380" unit="mm" val="380"></attnum>
    <attnum name="piston area" unit="cm2" val="50"></attnum>
    <attnum name="inertia" unit="kg.m2" val="0.1241"></attnum>
  </section>

  <section name="Front Left Brake">
    <attnum name="disk diameter" min="100" max="380" unit="mm" val="380"></attnum>
    <attnum name="piston area" unit="cm2" val="50"></attnum>
    <attnum name="inertia" unit="kg.m2" val="0.1241"></attnum>
  </section>

  <section name="Rear Right Brake">
    <attnum name="disk diameter" min="100" max="380" unit="mm" val="350"></attnum>
    <attnum name="piston area" unit="cm2" val="25"></attnum>
    <attnum name="inertia" unit="kg.m2" val="0.0714"></attnum>
  </section>

  <section name="Rear Left Brake">
    <attnum name="disk diameter" min="100" max="380" unit="mm" val="350"></attnum>
    <attnum name="piston area" unit="cm2" val="25"></attnum>
    <attnum name="inertia" unit="kg.m2" val="0.0714"></attnum>
