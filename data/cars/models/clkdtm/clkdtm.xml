    <attnum name="rim diameter" min="16" max="20" unit="in" val="18.0"></attnum>
    <attnum name="tire width" min="135" max="335" unit="mm" val="330"></attnum>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".30"></attnum>
    <attnum name="inertia" unit="kg.m2" val="1.2200"></attnum>
    <attnum name="camber" min="-10" max="0" unit="deg" val="-2"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"></attnum>
    <attnum name="toe" min="0" max="0" unit="deg" val="0"></attnum>
    <!-- Adherence -->
    <attnum name="stiffness" min="20.0" max="50.0" val="20.0"></attnum>
    <attnum name="dynamic friction" min="80" max="70" unit="%" val="80"></attnum>
    <attnum name="rolling resistance" val="0.02"></attnum>
    <attnum name="mu" val="1.7"></attnum>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" min="0" max="5000" unit="lbs/in" val="0"></attnum>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2.5"></attnum>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" min="0" max="5000" unit="lbs/in" val="0"></attnum>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2.5"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="5500"></attnum>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1"></attnum>
