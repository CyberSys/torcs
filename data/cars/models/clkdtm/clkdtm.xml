    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="80"></attnum>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="80"></attnum>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="100"></attnum>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="100"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="5500"></attnum>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1"></attnum>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"></attnum>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="80"></attnum>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="80"></attnum>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="100"></attnum>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="100"></attnum>
  </section>

  <section name="Rear Left Suspension">
