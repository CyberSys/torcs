    <attnum name="revs limiter" min="3000" max="10000" val="8500" unit="rpm"/>
    <attnum name="revs maxi" min="5000" max="10000" val="10000" unit="rpm"/>
    <attnum name="tickover" min="900" max="3000" val="1000" unit="rpm"/>
    <attnum name="fuel cons factor" min="1.1" max="1.3" val="1.2"/>
