<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Track-RWD-GrA" type="template">

  <section name="Car">
    <attstr name="category" val="Track-RWD-GrA"/>
    <attnum name="body length" unit="m" min="4.0" max="5.0" val="4.683"/>
    <attnum name="body width" unit="m" min="0.8" max="2.2" val="1.4"/>
    <attnum name="body height" unit="m" min="1.0" max="2.0" val="1.173"/>
    <!-- collision bounding box -->
    <attnum name="overall length" unit="m" min="4.0" max="5.0" val="4.7"/>
    <attnum name="overall width"  unit="m" min="1.2" max="2.2" val="1.7"/>
    <attnum name="mass" unit="kg" min="900.0" max="1500" val="1150.0"/>
    <attnum name="GC height" unit="m" min="0.1" max="1.0" val="0.30"/>
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" min="0.3" max="0.7" val="0.5"/>
    <attnum name="front right-left weight repartition" min="0.3" max="0.7" val="0.5"/>
    <attnum name="rear right-left weight repartition" min="0.3" max="0.7" val="0.5"/>
    <!-- used for inertia, indicates the good mass centering (lower values) -->
    <attnum name="mass repartition coefficient" min="0.2" max="1.0" val="0.9"/>
    <attnum name="fuel tank" unit="l" min="30.0" max="100.0" val="100.0"/>
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="5.0"/>
  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" min="0.20" max="2.0" val="0.38"/>
    <attnum name="front area" unit="m2" min="1.0" max="3.0" val="2.085"/>
    <attnum name="front Clift" min="0.0" max="1.5" val="0.6"/>
    <attnum name="rear Clift"  min="0.0" max="1.5" val="0.4"/>
  </section>

  <section name="Front Wing">
    <attnum name="area" unit="m2" min="0" max="1.5" val="0.5"/>
    <attnum name="angle" unit="deg" min="0" max="30" val="30"/>
    <attnum name="xpos" unit="m" min="0.0" max="2.5" val="2"/>
    <attnum name="zpos" unit="m" min="0.1" max="1.0" val="0.1"/>
