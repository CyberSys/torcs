<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="TORCS GrB" type="template">

  <section name="Car">
    <attnum name="body length" unit="m" min="4.5" max="5.0" val="4.683"></attnum>
    <attnum name="body width" unit="m" min="0.8" max="2.1" val="1.4"></attnum>
    <attnum name="body height" unit="m" min="1.1" max="1.7" val="1.173"></attnum>
    <!-- collision bounding box -->
    <attnum name="overall length" unit="m" min="4.5" max="5.0" val="4.7"></attnum>
    <attnum name="overall width"  unit="m" min="1.2" max="2.1" val="1.7"></attnum>
    <attnum name="mass" unit="kg" min="1100.0" max="2500" val="1550.0"></attnum>
    <attnum name="GC height" unit="m" min="0.15" max="0.5" val="0.30"></attnum>
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" min="0.4" max="0.6" val="0.5"></attnum>
    <attnum name="front right-left weight repartition" min="0.4" max="0.6" val="0.5"></attnum>
    <attnum name="rear right-left weight repartition" min="0.4" max="0.6" val="0.5"></attnum>
    <!-- used for inertia, indicates the good mass centering -->
    <attnum name="mass repartition coefficient" min="0.2" max="1.0" val="0.9"></attnum>
    <attnum name="fuel tank" unit="l" min="30.0" max="100.0" val="100.0"></attnum>
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="5.0"></attnum>
  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" min="0.20" max="2.0" val="0.38"></attnum>
    <attnum name="front area" unit="m2" min="1.0" max="3.0" val="2.085"></attnum>
    <attnum name="front Clift" min="0.0" max="1.0" val="0.3"></attnum>
    <attnum name="rear Clift"  min="0.0" max="1.0" val="0.2"></attnum>
  </section>

  <section name="Front Wing">
    <attnum name="area" unit="m2" min="0" max="0.5" val="0.5"></attnum>
    <attnum name="angle" unit="deg" min="0" max="30" val="30"></attnum>
    <attnum name="xpos" unit="m" min="1.0" max="2.5" val="2"></attnum>
    <attnum name="zpos" unit="m" min="0.1" max="0.5" val="0.1"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="area"  unit="m2" min="0" max="1.0" val="1.0"></attnum>
    <attnum name="angle" unit="deg" min="0" max="30" val="30"></attnum>
    <attnum name="xpos" unit="m" min="-2.5" max="-1.0" val="-2.3"></attnum>
    <attnum name="zpos" unit="m" min="0.1" max="1.5" val="1.1"></attnum>
  </section>

  <!-- Engine Max curve -->
  <section name="Engine">
    <attnum name="inertia"      unit="kg.m2" val="0.08"></attnum>
    <attnum name="revs limiter" min="3000" max="9500" unit="rpm" val="9000"></attnum>
    <attnum name="revs maxi" min="3000" max="10000" unit="rpm" val="10000"></attnum>
    <attnum name="tickover" min="500" max="5000" unit="rpm" val="2000"></attnum>
    <attnum name="fuel cons factor" min="1.0" max="2.0" val="1.2"></attnum>
    <section name="data points">
      <section name="1">
	<attnum name="rpm" unit="rpm" val="0"></attnum>
	<attnum name="Tq" min="0" max="280" unit="N.m" val="361"></attnum>
      </section>
      <section name="2">
	<attnum name="rpm" unit="rpm" val="500"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="380"></attnum>
      </section>
      <section name="3">
	<attnum name="rpm" unit="rpm" val="1000"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="437"></attnum>
      </section>
      <section name="4">
	<attnum name="rpm" unit="rpm" val="1500"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="474"></attnum>
      </section>
      <section name="5">
	<attnum name="rpm" unit="rpm" val="2000"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="518"></attnum>
      </section>
      <section name="6">
	<attnum name="rpm" unit="rpm" val="2500"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="518"></attnum>
      </section>
      <section name="7">
	<attnum name="rpm" unit="rpm" val="3000"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="520"></attnum>
      </section>
      <section name="8">
	<attnum name="rpm" unit="rpm" val="3500"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="529"></attnum>
      </section>
      <section name="9">
	<attnum name="rpm" unit="rpm" val="4000"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="531"></attnum>
      </section>
      <section name="10">
	<attnum name="rpm" unit="rpm" val="4500"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="532"></attnum>
      </section>
      <section name="11">
	<attnum name="rpm" unit="rpm" val="5000"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="532"></attnum>
      </section>
      <section name="12">
	<attnum name="rpm" unit="rpm" val="5500"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="517"></attnum>
      </section>
      <section name="13">
	<attnum name="rpm" unit="rpm" val="6000"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="493"></attnum>
      </section>
      <section name="14">
	<attnum name="rpm" unit="rpm" val="6500"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="463"></attnum>
      </section>
      <section name="15">
	<attnum name="rpm" unit="rpm" val="7000"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="460"></attnum>
      </section>
      <section name="16">
	<attnum name="rpm" unit="rpm" val="7500"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="450"></attnum>
      </section>
      <section name="17">
	<attnum name="rpm" unit="rpm" val="8000"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="440"></attnum>
      </section>
      <section name="18">
	<attnum name="rpm" unit="rpm" val="8500"></attnum>
	<attnum name="Tq" min="0"  unit="N.m" val="440"></attnum>
      </section>
      <section name="19">
	<attnum name="rpm" unit="rpm" val="9000"></attnum>
	<attnum name="Tq" min="0" unit="N.m" val="440"></attnum>
      </section>
      <section name="20">
	<attnum name="rpm" unit="rpm" val="9500"></attnum>
	<attnum name="Tq" min="0" unit="N.m" val="420"></attnum>
      </section>
      <section name="21">
	<attnum name="rpm" unit="rpm" val="10000"></attnum>
	<attnum name="Tq" min="0" unit="N.m" val="350"></attnum>
      </section>
    </section>
  </section>

  <section name="Clutch">
    <!-- pressure plate -->
    <attnum name="inertia" unit="kg.m2" val="0.0150"></attnum>
  </section>

  <section name="Gearbox">
    <attnum name="shift time" min=".10" max="0.30" val="0.1" unit="s"></attnum>
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-5" max="0" val="-2.0"></attnum>
	<attnum name="inertia" val="0.0037"></attnum>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.957"></attnum>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="2.9"></attnum>
	<attnum name="inertia" val="0.003"></attnum>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.955"></attnum>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="1.76"></attnum>
	<attnum name="inertia" val="0.0037"></attnum>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.957"></attnum>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.1"></attnum>
	<attnum name="inertia" val="0.0048"></attnum>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.950"></attnum>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="0.8"></attnum>
	<attnum name="inertia" val="0.0064"></attnum>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.983"></attnum>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.67"></attnum>
	<attnum name="inertia" val="0.0107"></attnum>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.948"></attnum>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.5"></attnum>
	<attnum name="inertia" val="0.0150"></attnum>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.930"></attnum>
      </section>
    </section>
  </section>

  <section name="Drivetrain">
    <!-- allowed values RWD, FWD, 4WD -->
    <attstr name="type"                 val="4WD"></attstr>
    <!-- drive shaft inertia -->
    <attnum name="inertia" unit="kg.m2" val="0.0091"></attnum>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="1" max="45" val="45"></attnum>
    <attnum name="max steer speed" unit="deg/s" min="1" max="360" val="120"></attnum>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.65"></attnum>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="10000"></attnum>
  </section>

  <section name="Front Axle">
    <attnum name="xpos" min="0.5" max="2.5" val="1.104"></attnum>
    <attnum name="inertia" unit="kg.m2"        val="0.0056"></attnum>
    <attnum name="roll center height" unit="m" min="0" max="0.5" val="0.012"></attnum>
  </section>

  <section name="Rear Axle">
    <attnum name="xpos" min="-2.5" max="-0.5" val="-1.469"></attnum>
    <attnum name="inertia" unit="kg.m2"        val="0.0080"></attnum>
    <attnum name="roll center height" unit="m" min="0" max="0.5" val="0.04"></attnum>
  </section>

  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="FREE"></attstr>
    <attnum name="inertia" unit="kg.m2" val="0.0488"></attnum>
    <attnum name="ratio" min="0" max="15" val="1.0"></attnum>
    <attnum name="efficiency" min=".5" max="1.0" val="0.9625"></attnum>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="FREE"></attstr>
    <attnum name="inertia" unit="kg.m2" val="0.0488"></attnum>
    <attnum name="ratio" min="0" max="15" val="1.0"></attnum>
    <attnum name="efficiency" min=".5" max="1.0" val="0.9625"></attnum>
  </section>

  <section name="Central Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="NONE,SPOOL,FREE,LIMITED SLIP,VISCOUS COUPLER" val="VISCOUS COUPLER"></attstr>
    <attnum name="min torque bias" val="50" unit="%"></attnum>
    <attnum name="max torque bias" val="60" unit="%"></attnum>
    <attnum name="inertia" min="0.001" max="0.1" val="0.0488" unit="kg.m2"></attnum>
    <attnum name="ratio" min="0" max="10" val="4.5"></attnum>
    <attnum name="efficiency" min="0.5" max="1.0" val="0.9625"></attnum>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ypos" unit="m" max="-0.5" min="-0.9" val="-0.81"></attnum>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" min="135" max="335" val="335"></attnum>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".30"></attnum>
    <attnum name="inertia" val="1.2200" unit="kg.m2" min="1"></attnum>
    <attnum name="mu" min="0.05" max="1.6" val="1.4"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"></attnum>
    <attnum name="rolling resistance" min="0.005" max="0.1" val="0.0105"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ypos" unit="m" min="0.5" max="0.9" val="0.81"></attnum>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" min="135" max="335" val="335"></attnum>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".30"></attnum>
    <attnum name="inertia" unit="kg.m2" val="1.2200" min="1"></attnum>
    <attnum name="mu" min="0.05" max="1.6" val="1.4"></attnum>
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"></attnum>
    <attnum name="rolling resistance" min="0.005" max="0.1" val="0.0105"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ypos" unit="m" max="-0.5" min="-0.9" val="-0.81"></attnum>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" min="135" max="335" val="335"></attnum>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".30"></attnum>
    <attnum name="inertia" unit="kg.m2" val="1.2200" min="1"></attnum>
    <attnum name="mu" min="0.05" max="1.6" val="1.4"></attnum>
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"></attnum>
    <attnum name="rolling resistance" min="0.005" max="0.1" val="0.0105"></attnum>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ypos" unit="m" min="0.5" max="0.9" val="0.81"></attnum>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" min="135" max="335" val="335"></attnum>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".30"></attnum>
    <attnum name="inertia" unit="kg.m2" val="1.2200" min="1"></attnum>
    <attnum name="mu" min="0.05" max="1.6" val="1.4"></attnum>
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"></attnum>
    <attnum name="rolling resistance" min="0.005" max="0.1" val="0.0105"></attnum>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="2000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.3" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2.5"></attnum>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="2000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.3" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2.5"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.3" val="0.2"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.3" val="0.2"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.3" val="0.2"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.3" val="0.2"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Front Right Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="380"></attnum>
    <attnum name="piston area"   unit="cm2"   val="50"></attnum>
    <attnum name="mu"                         val="0.45"></attnum>
    <attnum name="inertia"       unit="kg.m2" val="0.1241"></attnum>
  </section>

  <section name="Front Left Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="380"></attnum>
    <attnum name="piston area"   unit="cm2"   val="50"></attnum>
    <attnum name="mu"                         val="0.45"></attnum>
    <attnum name="inertia"       unit="kg.m2" val="0.1241"></attnum>
  </section>

  <section name="Rear Right Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="280"></attnum>
    <attnum name="piston area"   unit="cm2"   val="25"></attnum>
    <attnum name="mu"                         val="0.45"></attnum>
    <attnum name="inertia"       unit="kg.m2" val="0.0714"></attnum>
  </section>

  <section name="Rear Left Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="280"></attnum>
    <attnum name="piston area"   unit="cm2"   val="25"></attnum>
    <attnum name="mu"                         val="0.45"></attnum>
    <attnum name="inertia"       unit="kg.m2" val="0.0714"></attnum>
  </section>

</params>


