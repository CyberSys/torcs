 </section> <!-- Graphic -->

<section name="Main Track">
    <attstr name="surface" in="asphalt,grass,sand" val="asphalt2"></attstr>
    <attnum name="width" unit="m" val="15.0"></attnum>
    <attnum name="profil steps length" val="10" unit="m"></attnum>
    <section name="Right Side">
        <attstr name="surface"  val="as-pits"></attstr>
	<attnum name="width"    val="15.0"></attnum>
	<attstr name="banking type" in="level,tangent" val="tangent"></attstr>
    </section>
    <section name="Left Side">
        <attstr name="surface"  val="asphaltl"></attstr>
	<attnum name="width"    val="5.0"></attnum>
	<attstr name="banking type" in="level,tangent" val="tangent"></attstr>
    </section>
    <section name="Right Border">
    <attnum name="width" val="1.0"></attnum>
    <attstr name="surface" val="g-grass3-l"></attstr>
    </section>
    <section name="Left Border">
    <attnum name="width" val="1.0"></attnum>
    <attstr name="surface" val="g-grass3-l"></attstr>
    </section>

    <section name="Pits">
    <attstr name="side"   val="right"></attstr>
    <attstr name="entry"  val="segment line 6"></attstr>
    <attstr name="start"  val="last segment"></attstr>
    <attstr name="end"    val="start lane"></attstr>
    <attstr name="exit"   val="s3"></attstr>
    <attnum name="length" val="15" unit="m"></attnum>
    <attnum name="width"  val="5" unit="m"></attnum>
    </section>  <!-- PITS -->

    <section name="Track Segments">

      <section name="start lane">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="50"></attnum>

	<section name="Left Side">
	<attstr name="surface"  val="asphaltl"></attstr>
	<attnum name="width"    val="5.0"></attnum>
	<attstr name="banking type" in="level,tangent" val="tangent"></attstr>
	</section>

	<section name="Right Barrier">
	  <attstr name="style" val="fence"></attstr>
	  <attnum name="height" val="5.0"></attnum>	  
	  <attstr name="surface" val="barrierg1"></attstr>
	</section>
	<section name="Left Barrier">
	  <attstr name="style" val="fence"></attstr>
	  <attnum name="height" val="5.0"></attnum>	
	  <attstr name="surface" val="barrierg1"></attstr>  
	</section>
      </section>
      <section name="s3">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="50"></attnum>
        <attnum name="texture size"    val="100.0"></attnum>
      </section>

      <section name="segment line1">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="86.01"></attnum>
	<attnum name="z end" val="-4.0" unit="m"></attnum>
	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" in="linear,spline" val="spline"></attstr>
	<attnum name="profil steps" val="20"></attnum>

	<section name="Left Side">
	<attstr name="surface" val="gconcrete"></attstr>
	<attnum name="width" val="5"></attnum>
	<attstr name="banking type" in="level,tangent" val="tangent"></attstr>

	</section>
	<section name="Right Side">
	<attstr name="surface" val="asphaltr"></attstr>
	<attnum name="width" val="15"></attnum>
	<attstr name="banking type" in="level,tangent" val="tangent"></attstr>
	</section>

        <attnum name="texture size"    val="100.0"></attnum>

      </section>
      <section name="turn 1">
        <attstr name="type" val="rgt"></attstr>
	<attnum name="radius" unit="m" val="200"></attnum>
	<attnum name="arc" unit="deg" val="30"></attnum>
	<attnum name="z end" val="-5.0" unit="m"></attnum>
	<attstr name="profil" in="linear,spline" val="spline"></attstr>
	<attnum name="profil steps" val="8"></attnum>

	<section name="Left Side">
	<attstr name="surface" val="gconcrete"></attstr>
	<attnum name="width" val="5"></attnum>
	<attstr name="banking type" in="level,tangent" val="tangent"></attstr>
	</section>	
	<section name="Right Side">
	<attstr name="surface" val="asphaltr"></attstr>
	<attnum name="width" val="15"></attnum>
	<attstr name="banking type" in="level,tangent" val="tangent"></attstr>
	</section>
	<section name="Right Barrier">
	  <attstr name="style" val="fence"></attstr>
	  <attnum name="height" val="1.0"></attnum>	
	  <attstr name="surface" val="barrierg2"></attstr>  
	</section>

	<attnum name="banking end" val="0" unit="deg"></attnum>
        <attnum name="texture size"    val="100.0"></attnum>
      </section>

      <section name="segment line2">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="150"></attnum>
	<attnum name="z end" val="-10" unit="m"></attnum>
	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" in="linear,spline" val="spline"></attstr>
	<attnum name="profil steps" val="20"></attnum>

