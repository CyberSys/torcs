  <section name="Header">
    <attstr name="name"        val="E-Track 4"/>
    <attstr name="category"    val="road"/>
    <attnum name="version"     val="3"/>
    <attstr name="author"      val="E.Espie"/>
    <attstr name="description" val="High speed track"/>
