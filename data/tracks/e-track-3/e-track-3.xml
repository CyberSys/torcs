<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd" [
<!-- general definitions for tracks -->
<!ENTITY default-surfaces SYSTEM "data/tracks/surfaces.xml">
]>

<params name="E-Track 3" type="trackdef" mode="m">

  <section name="Surfaces">
    <section name="List">
      &default-surfaces;
    </section>
  </section>

  <section name="Header">
    <attstr name="name"        val="E-Track 3"></attstr>
    <attnum name="version"     val="2"></attnum>
    <attstr name="author"      val="E.Espie"></attstr>
    <attstr name="description" val="Another Track"></attstr>
  </section>

  <section name="Graphic">
    <attstr name="3d description" val="e-track-3.ac"></attstr>

    <attstr name="background image" val="background.png"></attstr>
    <attnum name="background color R" val="0.28125"></attnum>
    <attnum name="background color G" val="0.4375"></attnum>
    <attnum name="background color B" val="0.75"></attnum>

    <section name="Environment Mapping">
      <section name="1">
	<attstr name="env map image" val="env.rgb"></attstr>
      </section>
    </section>

    <section name="Terrain Generation">
      <attnum name="track step" unit="m" val="15"></attnum>
      <attnum name="border margin" unit="m" val="100"></attnum>
      <attnum name="border step" unit="m" val="40"></attnum>
      <attnum name="border height" unit="m" val="0"></attnum>
      <attstr name="orientation" val="clockwise"></attstr>
      <attstr name="relief file" val="e-track-3-relief.ac"></attstr>
    </section>

    <section name="Turn Marks">
      <attnum name="width" val="3.0" unit="m"></attnum>
      <attnum name="height" val="2.0" unit="m"></attnum>
      <attnum name="vertical space" val="1.0" unit="m"></attnum>
      <attnum name="horizontal space" val="0.0" unit="m"></attnum>
    </section>
  </section>

  <section name="Main Track">

    <attstr name="surface" in="asphalt,grass,sand" val="asphalt-lines"></attstr>
    <attnum name="width" unit="m" val="18.0"></attnum>

    <attstr name="lside surface"  val="grass"></attstr>
    <attnum name="lside width"    val="5.0"></attnum>
    <attstr name="lside type" in="tangent,level" val="tangent"></attstr>

    <attstr name="rside surface"  val="asphalt-pits"></attstr>
    <attnum name="rside width"    val="15.0"></attnum>
    <attstr name="rside type" in="tangent,level" val="tangent"></attstr>

    <attnum name="profil steps length" val="12" unit="m"></attnum>

    <!-- PITS -->
    <attstr name="pit type"   val="track side"></attstr>
    <attstr name="pit side"   val="right"></attstr>
    <attstr name="pit entry"  val="s15-1"></attstr>
    <attstr name="pit exit"   val="s3"></attstr>
    <attstr name="pit start"  val="last segment"></attstr>
    <attstr name="pit end"    val="start lane"></attstr>
    <attnum name="pit length" val="15" unit="m"></attnum>
    <attnum name="pit width"  val="5" unit="m"></attnum>

    <section name="segments">

      <section name="start lane">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="180"></attnum>
	<attnum name="profil steps" val="12"></attnum>
      </section>

      <section name="s1">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="25"></attnum>
	<attstr name="rside surface" val="asphalt"></attstr>
	<attnum name="rside end width" val="10"></attnum>
      </section>

      <section name="s2">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="45"></attnum>
     </section>

      <section name="s3">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="150"></attnum>
	<attnum name="rside end width" val="0"></attnum>
	<attstr name="profil" in="linear,spline" val="spline"></attstr>
	<attnum name="profil steps" val="10"></attnum>
	<attnum name="banking end" val="-3" unit="deg"></attnum>
      </section>

      <section name="t1">
        <attstr name="type" val="lft"></attstr>
	<attnum name="radius" unit="m" val="60"></attnum>
	<attnum name="arc" unit="deg" val="30"></attnum>
	<attstr name="rside surface" val="grass"></attstr>
	<attnum name="rside end width" val="5"></attnum>
	<attstr name="surface" val="asphalt-l-left"></attstr>
      </section>

      <section name="s4">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="200"></attnum>
	<attnum name="grade" val="4" unit="%"></attnum>
	<attstr name="profil" in="linear,spline" val="spline"></attstr>
	<attnum name="profil steps" val="40"></attnum>
	<attnum name="profil end tangent" val="4" unit="%"></attnum>
	<attnum name="banking end" val="3" unit="deg"></attnum>
	<attstr name="surface" val="asphalt-lines"></attstr>
      </section>

      <section name="t2">
        <attstr name="type" val="rgt"></attstr>
	<attnum name="radius" unit="m" val="60"></attnum>
	<attnum name="arc" unit="deg" val="70"></attnum>
	<attstr name="surface" val="asphalt-l-right"></attstr>
	<attstr name="marks" val="50;100"></attstr>
      </section>

      <section name="s5">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="100"></attnum>
	<attnum name="grade" val="4" unit="%"></attnum>
	<attstr name="profil" in="linear,spline" val="spline"></attstr>
	<attnum name="profil steps" val="20"></attnum>
	<attnum name="profil end tangent" val="0" unit="%"></attnum>
	<attstr name="surface" val="asphalt-lines"></attstr>
      </section>

      <section name="t3">
        <attstr name="type" val="rgt"></attstr>
	<attnum name="radius" unit="m" val="60"></attnum>
	<attnum name="arc" unit="deg" val="75"></attnum>
	<attnum name="grade" val="0" unit="%"></attnum>
	<attstr name="surface" val="asphalt-l-right"></attstr>
	<attstr name="marks" val="50"></attstr>
      </section>

      <section name="s6">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="100"></attnum>
	<attnum name="grade" val="-10" unit="%"></attnum>
	<attstr name="profil" in="linear,spline" val="spline"></attstr>
	<attnum name="profil steps" val="20"></attnum>
	<attnum name="profil end tangent" val="-10" unit="%"></attnum>
	<attstr name="surface" val="asphalt-lines"></attstr>
      </section>

      <section name="t4">
        <attstr name="type" val="rgt"></attstr>
	<attnum name="radius" unit="m" val="60"></attnum>
	<attnum name="arc" unit="deg" val="60"></attnum>
	<attstr name="surface" val="asphalt-l-right"></attstr>
      </section>

      <section name="s7-1">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="150"></attnum>
	<attnum name="grade" val="-10" unit="%"></attnum>
	<attstr name="profil" in="linear,spline" val="spline"></attstr>
	<attnum name="profil steps" val="30"></attnum>
	<attnum name="profil end tangent" val="0" unit="%"></attnum>
	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="surface" val="asphalt-lines"></attstr>
      </section>

      <section name="s7-2">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="100"></attnum>
	<attnum name="grade" val="5" unit="%"></attnum>
	<attstr name="profil" in="linear,spline" val="spline"></attstr>
	<attnum name="profil steps" val="20"></attnum>
	<attnum name="profil end tangent" val="0" unit="%"></attnum>
	<attnum name="banking end" val="-3" unit="deg"></attnum>
      </section>

      <section name="t5-1">
        <attstr name="type" val="lft"></attstr>
	<attnum name="radius" unit="m" val="40"></attnum>
	<attnum name="arc" unit="deg" val="110"></attnum>
	<attnum name="grade" val="0" unit="%"></attnum>
	<attstr name="profil" in="linear,spline" val="spline"></attstr>
	<attnum name="profil steps" val="10"></attnum>
	<attnum name="banking end" val="3" unit="deg"></attnum>
	<attstr name="surface" val="asphalt-l-left"></attstr>
	<attstr name="marks" val="50;100;150"></attstr>
	<attstr name="rside surface" val="concrete"></attstr>
	<attstr name="lside surface" val="concrete"></attstr>
      </section>

      <section name="t5-2">
        <attstr name="type" val="rgt"></attstr>
	<attnum name="radius" unit="m" val="30"></attnum>
	<attnum name="arc" unit="deg" val="97"></attnum>
	<attstr name="profil" in="linear,spline" val="spline"></attstr>
	<attnum name="profil steps" val="10"></attnum>
	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="surface" val="asphalt-l-right"></attstr>
      </section>

      <section name="s8-1">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="50"></attnum>
