	<attnum name="arc" unit="deg" val="35"></attnum>
	<attnum name="rside end width" val="0"></attnum>
	<attstr name="surface" val="asphalt-l-right"></attstr>
      </section>

      <section name="s15-1">
        <attstr name="type" val="str"></attstr>
