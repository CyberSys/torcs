      </section>

      <section name="cam 7">
	<!-- Camera's position relative to a segment -->
	<attstr name="segment"   val="t7-2"></attstr>
	<attnum name="to right"  val="25.0"></attnum>
	<attnum name="to start"  val="60" unit="deg"></attnum>
