  <section name="Cameras">
    <section name="list">


      <section name="cam0">
	<!-- Camera's position relative to a segment -->
	<attstr name="segment"   val="s3"></attstr>
	<attnum name="to right"  val="0.0"></attnum>
	<attnum name="to start"  val="50"></attnum>
	<attnum name="height"    val="2"></attnum>
	<!-- Camera field of view, indicates the first and last segment viewed -->
	<attstr name="fov start" val="last segment"></attstr>
	<attstr name="fov end"   val="t1"></attstr>	
      </section>


      <section name="cam 1">
	<!-- Camera's position relative to a segment -->
	<attstr name="segment"   val="t1"></attstr>
	<attnum name="to right"  val="-5.0"></attnum>
	<attnum name="to start"  val="30" unit="deg"></attnum>
	<attnum name="height"    val="2"></attnum>
	<!-- Camera field of view, indicates the first and last segment viewed -->
	<attstr name="fov start" val="t1"></attstr>
	<attstr name="fov end"   val="t2"></attstr>	
      </section>

      <section name="cam 2">
	<!-- Camera's position relative to a segment -->
	<attstr name="segment"   val="t3"></attstr>
	<attnum name="to right"  val="0.0"></attnum>
	<attnum name="to start"  val="30" unit="deg"></attnum>
	<attnum name="height"    val="3"></attnum>
	<!-- Camera field of view, indicates the first and last segment viewed -->
	<attstr name="fov start" val="t2"></attstr>
	<attstr name="fov end"   val="s6"></attstr>	
      </section>

      <section name="cam 3">
	<!-- Camera's position relative to a segment -->
	<attstr name="segment"   val="t4"></attstr>
	<attnum name="to right"  val="-5.0"></attnum>
	<attnum name="to start"  val="30" unit="deg"></attnum>
	<attnum name="height"    val="2"></attnum>
	<!-- Camera field of view, indicates the first and last segment viewed -->
	<attstr name="fov start" val="s6"></attstr>
	<attstr name="fov end"   val="s7-2"></attstr>	
      </section>

      <section name="cam 4">
	<!-- Camera's position relative to a segment -->
	<attstr name="segment"   val="s7-2"></attstr>
	<attnum name="to right"  val="0.0"></attnum>
	<attnum name="to start"  val="100"></attnum>
	<attnum name="height"    val="6"></attnum>
	<!-- Camera field of view, indicates the first and last segment viewed -->
	<attstr name="fov start" val="s7-2"></attstr>
	<attstr name="fov end"   val="t5-1"></attstr>	
      </section>

      <section name="cam 4">
	<!-- Camera's position relative to a segment -->
	<attstr name="segment"   val="t5-2"></attstr>
	<attnum name="to right"  val="23.0"></attnum>
	<attnum name="to start"  val="30" unit="deg"></attnum>
	<attnum name="height"    val="6"></attnum>
	<!-- Camera field of view, indicates the first and last segment viewed -->
	<attstr name="fov start" val="t5-1"></attstr>
	<attstr name="fov end"   val="s8-2"></attstr>	
      </section>

      <section name="cam 5">
	<!-- Camera's position relative to a segment -->
	<attstr name="segment"   val="t6"></attstr>
	<attnum name="to right"  val="-5.0"></attnum>
	<attnum name="to start"  val="8" unit="deg"></attnum>
	<attnum name="height"    val="5"></attnum>
	<!-- Camera field of view, indicates the first and last segment viewed -->
	<attstr name="fov start" val="s8-2"></attstr>
	<attstr name="fov end"   val="s9"></attstr>	
      </section>

      <section name="cam 6">
	<!-- Camera's position relative to a segment -->
	<attstr name="segment"   val="t7-1"></attstr>
	<attnum name="to right"  val="25.0"></attnum>
	<attnum name="to start"  val="0" unit="deg"></attnum>
	<attnum name="height"    val="5"></attnum>
	<!-- Camera field of view, indicates the first and last segment viewed -->
