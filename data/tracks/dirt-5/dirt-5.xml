<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd" [
<!-- general definitions for tracks -->
<!ENTITY default-surfaces SYSTEM "data/tracks/surfaces.xml">
]>

<params name="dirt 5" type="trackdef" mode="m">

  <section name="Surfaces">
    <section name="List">
      &default-surfaces;
    </section>
  </section>

  <section name="Header">
    <attstr name="name"        val="Dirt 5"></attstr>
    <attstr name="category"    val="dirt"></attstr>
    <attnum name="version"     val="3"></attnum>
    <attstr name="author"      val="E.Espie"></attstr>
    <attstr name="description" val=""></attstr>
  </section>

  <section name="Graphic">
    <attstr name="3d description" val="dirt-5.ac"></attstr>
    <attstr name="background image" val="background.png"></attstr>
    <attnum name="background color R" val="0.59375"></attnum>
    <attnum name="background color G" val="0.75"></attnum>
    <attnum name="background color B" val="0.96875"></attnum>

    <section name="Environment Mapping">
      <section name="general">
	<attstr name="env map image" val="env.png"></attstr>
      </section>
    </section>

    <section name="Terrain Generation">
      <attnum name="track step" unit="m" val="5"></attnum>
      <attnum name="border margin" unit="m" val="100"></attnum>
      <attnum name="border step" unit="m" val="10"></attnum>
      <attnum name="border height" unit="m" val="10"></attnum>
      <attstr name="orientation" val="clockwise"></attstr>
    </section>

  </section>

  <section name="Main Track">

    <attstr name="surface" val="dirt"></attstr>
    <attnum name="width" unit="m" val="10.0"></attnum>

    <attstr name="rside surface"  val="dirt-b"></attstr>
    <attnum name="rside width"    val="4.0"></attnum>
    <attstr name="rside type" in="tangent,level" val="level"></attstr>

    <attstr name="lside surface"  val="dirt-b"></attstr>
    <attnum name="lside width"    val="4.0"></attnum>
    <attstr name="lside type" in="tangent,level" val="level"></attstr>

    <section name="segments">

      <section name="start lane">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="40"></attnum>
	<attnum name="z end" unit="m" val="2"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="8"></attnum>
      </section>

      <section name="start lane2">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="40"></attnum>
	<attnum name="z end" unit="m" val="0"></attnum>
 	<attnum name="banking end" val="4" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="8"></attnum>
      </section>

      <section name="t1">
        <attstr name="type" val="rgt"></attstr>
	<attnum name="radius" unit="m" val="80"></attnum>
	<attnum name="arc" unit="deg" val="40"></attnum>
 	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="10"></attnum>
      </section>

      <section name="s1-1">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="30"></attnum>
	<attnum name="z end" unit="m" val="-1.0"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="8"></attnum>
      </section>

      <section name="s1-2">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="40"></attnum>
	<attnum name="z end" unit="m" val="0"></attnum>
 	<attnum name="banking end" val="5" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="8"></attnum>
      </section>

      <section name="t2">
        <attstr name="type" val="rgt"></attstr>
	<attnum name="radius" unit="m" val="30"></attnum>
	<attnum name="arc" unit="deg" val="105"></attnum>
 	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="10"></attnum>
	<attnum name="grade" val="3" unit="%"></attnum>
	<attnum name="profil end tangent" val="3" unit="%"></attnum>
      </section>

      <section name="s2">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="40"></attnum>
 	<attnum name="banking end" val="4" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="8"></attnum>
	<attnum name="grade" val="5" unit="%"></attnum>
	<attnum name="profil end tangent" val="5" unit="%"></attnum>
      </section>

      <section name="t3">
        <attstr name="type" val="rgt"></attstr>
	<attnum name="radius" unit="m" val="24"></attnum>
	<attnum name="arc" unit="deg" val="65"></attnum>
 	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="7"></attnum>
	<attnum name="grade" val="3" unit="%"></attnum>
	<attnum name="profil end tangent" val="3" unit="%"></attnum>
      </section>

      <section name="s3">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="80"></attnum>
 	<attnum name="banking end" val="-4" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="10"></attnum>
	<attnum name="grade" val="7" unit="%"></attnum>
	<attnum name="profil end tangent" val="7" unit="%"></attnum>
      </section>

      <section name="t4">
        <attstr name="type" val="lft"></attstr>
	<attnum name="radius" unit="m" val="40"></attnum>
	<attnum name="arc" unit="deg" val="80"></attnum>
 	<attnum name="banking end" val="4" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="8"></attnum>
	<attnum name="grade" val="4" unit="%"></attnum>
	<attnum name="profil end tangent" val="4" unit="%"></attnum>
      </section>

      <section name="t5">
        <attstr name="type" val="rgt"></attstr>
	<attnum name="radius" unit="m" val="60"></attnum>
	<attnum name="arc" unit="deg" val="40"></attnum>
 	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="5"></attnum>
	<attnum name="grade" val="4" unit="%"></attnum>
	<attnum name="profil end tangent" val="0" unit="%"></attnum>
      </section>

      <section name="s5-1">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="40"></attnum>
 	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="8"></attnum>
	<attnum name="grade" val="6" unit="%"></attnum>
	<attnum name="profil end tangent" val="0" unit="%"></attnum>
      </section>

      <section name="s5-1">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="40"></attnum>
 	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="8"></attnum>
	<attnum name="grade" val="-6" unit="%"></attnum>
	<attnum name="profil end tangent" val="0" unit="%"></attnum>
      </section>

      <section name="t6">
        <attstr name="type" val="lft"></attstr>
	<attnum name="radius" unit="m" val="40"></attnum>
	<attnum name="arc" unit="deg" val="40"></attnum>
 	<attnum name="banking end" val="3" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="5"></attnum>
	<attnum name="grade" val="0" unit="%"></attnum>
	<attnum name="profil end tangent" val="0" unit="%"></attnum>
      </section>

      <section name="t7-1">
        <attstr name="type" val="rgt"></attstr>
	<attnum name="radius" unit="m" val="60"></attnum>
	<attnum name="arc" unit="deg" val="140"></attnum>
 	<attnum name="banking end" val="3" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="15"></attnum>
	<attnum name="grade" val="-5" unit="%"></attnum>
	<attnum name="profil end tangent" val="-5" unit="%"></attnum>
      </section>

      <section name="s7">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="34.982437"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="8"></attnum>
	<attnum name="grade" val="-5" unit="%"></attnum>
	<attnum name="profil end tangent" val="-5" unit="%"></attnum>
      </section>

      <section name="t7-2">
        <attstr name="type" val="rgt"></attstr>
	<attnum name="radius" unit="m" val="60"></attnum>
	<attnum name="arc" unit="deg" val="40"></attnum>
 	<attnum name="banking end" val="3" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="8"></attnum>
	<attnum name="grade" val="-5" unit="%"></attnum>
	<attnum name="profil end tangent" val="-5" unit="%"></attnum>
      </section>

      <section name="t7-3">
        <attstr name="type" val="rgt"></attstr>
	<attnum name="radius" unit="m" val="80"></attnum>
	<attnum name="arc" unit="deg" val="50"></attnum>
 	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="10"></attnum>
	<attnum name="grade" val="-4" unit="%"></attnum>
	<attnum name="profil end tangent" val="-2" unit="%"></attnum>
      </section>

      <section name="t8">
        <attstr name="type" val="lft"></attstr>
	<attnum name="radius" unit="m" val="40"></attnum>
	<attnum name="arc" unit="deg" val="20"></attnum>
 	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="5"></attnum>
	<attnum name="grade" val="-2" unit="%"></attnum>
	<attnum name="profil end tangent" val="-2" unit="%"></attnum>
      </section>

      <section name="s8">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="20"></attnum>
 	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="5"></attnum>
	<attnum name="grade" val="-2" unit="%"></attnum>
	<attnum name="profil end tangent" val="-2" unit="%"></attnum>
      </section>

      <section name="t9">
        <attstr name="type" val="rgt"></attstr>
	<attnum name="radius" unit="m" val="40"></attnum>
	<attnum name="arc" unit="deg" val="20"></attnum>
 	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="5"></attnum>
	<attnum name="grade" val="-2" unit="%"></attnum>
	<attnum name="profil end tangent" val="0" unit="%"></attnum>
      </section>

      <section name="s9-1">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="30"></attnum>
 	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="8"></attnum>
	<attnum name="grade" val="5" unit="%"></attnum>
	<attnum name="profil end tangent" val="0" unit="%"></attnum>
      </section>

      <section name="s9-1">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="30"></attnum>
 	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="8"></attnum>
	<attnum name="grade" val="-5" unit="%"></attnum>
	<attnum name="profil end tangent" val="0" unit="%"></attnum>
      </section>

      <section name="s9">
        <attstr name="type" val="str"></attstr>
	<attnum name="lg" unit="m" val="57.996521"></attnum>
 	<attnum name="banking end" val="0" unit="deg"></attnum>
	<attstr name="profil" val="spline"></attstr>
	<attnum name="profil steps" val="12"></attnum>
	<attnum name="profil end tangent" val="0" unit="%"></attnum>
	<attnum name="z end" unit="m" val="0"></attnum>
      </section>

    </section>

  </section>

</params>


