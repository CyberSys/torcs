<!DOCTYPE params SYSTEM "../../../libs/tgf/params.dtd">

<params type="" name="damned2">


  <section name="Car">
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" min="0.3" max="0.7" val="0.45"></attnum>
    <attnum name="front right-left weight repartition" min="0.3" max="0.7" val="0.5"></attnum>
    <attnum name="rear right-left weight repartition" min="0.3" max="0.7" val="0.5"></attnum>
    <attnum name="initial fuel" unit="l" min="1.0" max="30.0" val="30.0"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="3"></attnum>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"></attnum>
	<attnum name="inertia" val="0.0037"></attnum>
	<attnum name="efficiency" val="0.957"></attnum>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="8" val="3.0"></attnum>
	<attnum name="inertia" val="0.003"></attnum>
	<attnum name="efficiency" val="0.955"></attnum>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="8" val="2.0"></attnum>
	<attnum name="inertia" val="0.0037"></attnum>
	<attnum name="efficiency" val="0.957"></attnum>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="8" val="1.5"></attnum>
	<attnum name="inertia" val="0.0048"></attnum>
	<attnum name="efficiency" val="0.950"></attnum>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="8" val="1.0"></attnum>
	<attnum name="inertia" val="0.0064"></attnum>
	<attnum name="efficiency" val="0.983"></attnum>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="8" val="0.8"></attnum>
	<attnum name="inertia" val="0.0107"></attnum>
	<attnum name="efficiency" val="0.948"></attnum>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="8" val="0.6"></attnum>
	<attnum name="inertia" val="0.0150"></attnum>
	<attnum name="efficiency" val="0.940"></attnum>
      </section>
    </section>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE" val="SPOOL"></attstr>
    <attnum name="ratio" min="0" max="8" val="4.5"></attnum>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="1" max="45" val="45"></attnum>
    <attnum name="max steer speed" unit="deg/s" min="1" max="360" val="120"></attnum>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.55"></attnum>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="10000"></attnum>
  </section>

  <section name="Front Right Wheel">
    <attnum name="rim diameter" unit="in" min="13" max="18" val="15.0"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="800" val="250"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Front Left Wheel">
    <attnum name="rim diameter" unit="in" min="13" max="18" val="15.0"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="800" val="250"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="rim diameter" unit="in" min="13" max="18" val="15.0"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="800" val="250"></attnum>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="rim diameter" unit="in" min="13" max="18" val="15.0"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="800" val="250"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1200"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="150"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1200"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="150"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

</params>


