<!DOCTYPE params SYSTEM "../../../libs/tgf/params.dtd">

<params name="Peugeot 406" type="parameter">

  <section name="Car">
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" min="0.4" max="0.7" val="0.60"></attnum>
    <attnum name="front right-left weight repartition" min="0.3" max="0.7" val="0.5"></attnum>
    <attnum name="rear right-left weight repartition" min="0.3" max="0.7" val="0.5"></attnum>
    <!-- used for inertia, indicates the good mass centering -->
    <attnum name="initial fuel" val="100.0" unit="l" min="1.0" max="100.0"></attnum>
  </section>

  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="10"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="25"></attnum>
  </section>


  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.65"></attnum>
    <attnum name="max pressure" unit="kPa" min="1000" max="500000" val="14000"></attnum>
  </section>

  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE" val="FREE"></attstr>
    <attnum name="ratio" min="0" max="5" val="4.0"></attnum>
  </section>

  <section name="Front Right Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="100" max="1000" val="145"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
    <attnum name="rim diameter" min="15" max="17" val="17.0" unit="in"></attnum>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"></attnum>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" unit="mm" min="100" max="1000" val="145"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
    <attnum name="rim diameter" min="15" max="17" val="17.0" unit="in"></attnum>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"></attnum>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" unit="mm" min="100" max="1000" val="150"></attnum>
    <attnum name="rim diameter" min="15" max="17" val="17.0" unit="in"></attnum>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"></attnum>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" unit="mm" min="100" max="1000" val="150"></attnum>
   <attnum name="rim diameter" min="15" max="17" val="17.0" unit="in"></attnum>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"></attnum>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="2000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1"></attnum>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="1000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.5" val="0.35"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="140"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="140"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.5" val="0.35"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="140"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="140"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.5" val="0.2"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="120"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="150"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="120"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="150"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.5" val=".2"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="120"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="150"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="120"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="150"></attnum>
  </section>




</params>


