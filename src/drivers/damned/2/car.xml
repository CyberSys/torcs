<!DOCTYPE params SYSTEM "../../../libs/tgf/params.dtd">

<params type="" name="damned2">


  <section name="Car">
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" min="0.3" max="0.7" val="0.45"></attnum>
    <attnum name="front right-left weight repartition" min="0.3" max="0.7" val="0.5"></attnum>
    <attnum name="rear right-left weight repartition" min="0.3" max="0.7" val="0.5"></attnum>
    <!-- used for inertia, indicates the good mass centering (lower values) -->
    <attnum name="initial fuel" unit="l" val="50.0"></attnum>
  </section>

  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="80" val="10"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="80" val="1"></attnum>
  </section>


  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.55"></attnum>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="13000"></attnum>
  </section>

  <section name="Front Right Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="800" val="100"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"></attnum>
  </section>

  <section name="Front Left Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="800" val="100"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"></attnum>
  </section>

  <section name="Rear Right Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="800" val="100"></attnum>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"></attnum>
  </section>

  <section name="Rear Left Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="800" val="100"></attnum>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1100"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.2"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.20"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="90"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="90"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1100"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.2"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.20"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="90"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="90"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1500"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.2"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.20"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1500"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.2"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.20"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

</params>


