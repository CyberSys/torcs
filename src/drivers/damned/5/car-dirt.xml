<!DOCTYPE params SYSTEM "../../../../libs/tgf/params.dtd">

<params name="damned 4" type="template" mode="mw">

  <section name="Car">
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" min="0.3" max="0.7" val="0.55"/>
    <attnum name="front right-left weight repartition" min="0.3" max="0.7" val="0.5"/>
    <attnum name="rear right-left weight repartition" min="0.3" max="0.7" val="0.5"/>
    <!-- used for inertia, indicates the good mass centering (lower values) -->
    <attnum name="initial fuel" unit="l" min="1.0" max="30.0" val="30.0"/>
  </section>

  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="80" val="60"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="80" val="40"/>
  </section>


  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="NONE,SPOOL,FREE,LIMITED SLIP" val="FREE"/>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="NONE,SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"/>
  </section>

  <section name="Central Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="NONE,SPOOL,FREE,LIMITED SLIP,VISCOUS COUPLER" val="VISCOUS COUPLER"/>
    <attnum name="min torque bias" min="20" max="50" unit="%" val="20"/>
    <attnum name="max torque bias" min="20" max="80" unit="%" val="30"/>
    <attnum name="ratio" min="0" max="10" val="6.85"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.45"/>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="15000"/>
  </section>

  <section name="Front Right Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="800" val="160"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-3"/>
  </section>

  <section name="Front Left Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="800" val="160"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-3"/>
  </section>

  <section name="Rear Right Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="800" val="160"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-3"/>
  </section>

  <section name="Rear Left Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="800" val="160"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-3"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1500"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1"/>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.30"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="70"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="1000"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="400"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="1000"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1500"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1"/>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.30"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="70"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="1000"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="400"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="1000"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3500"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1"/>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.30"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="1000"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="500"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="1000"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3500"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1"/>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.30"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="1000"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="500"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="1000"/>
  </section>


</params>


