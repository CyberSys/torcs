<!DOCTYPE params SYSTEM "../../../libs/tgf/params.dtd">

<params name="Tanhoj" type="template" mode="mw">
  <section name="Car">
    <!-- weight bias -->
    <attnum name="front right-left weight repartition" min="0.4" max="0.6" val="0.5"/>
    <attnum name="rear right-left weight repartition" min="0.4" max="0.6" val="0.5"/>
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="100.0"/>
  </section>

  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="25"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="10"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"/>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="3.5"/>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="2.15"/>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.56"/>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.21"/>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.97"/>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.85"/>
      </section>
    </section>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" val="FREE"/>
    <attnum name="ratio" val="4.2"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.50"/>
    <attnum name="max pressure" unit="kPa" val="25000"/>
  </section>

  <section name="Front Right Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="100" max="200" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
  </section>

  <section name="Front Left Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="100" max="200" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
  </section>

  <section name="Rear Right Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="100" max="200" val="100"/>
  </section>

  <section name="Rear Left Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="100" max="200" val="100"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"/>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.2"/>
    <attnum name="bellcrank" min="0.5" max="5" val="1"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"/>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.2"/>
    <attnum name="bellcrank" min="0.5" max="5" val="1"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2000"/>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.2"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"/>
    <attnum name="packers" unit="mm" min="0" max="100" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="60"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="60"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2000"/>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.2"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"/>
    <attnum name="packers" unit="mm" min="0" max="100" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="60"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="60"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3000"/>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.2"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.6"/>
    <attnum name="packers" unit="mm" min="0" max="100" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="60"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3000"/>
    <attnum name="suspension course" unit="m" min="0" max="1.0" val="0.2"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.6"/>
    <attnum name="packers" unit="mm" min="0" max="100" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="60"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"/>
  </section>

</params>



