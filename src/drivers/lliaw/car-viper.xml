<!DOCTYPE params SYSTEM "../../../libs/tgf/params.dtd">

<params name="Viper GTS-R" type="template">


  <section name="Car">
    <attstr name="category" val="torcs-r-a"></attstr>

    <attnum name="body length" unit="m" min="4.5" max="5.0" val="4.597"></attnum>
    <attnum name="body width" unit="m" min="0.8" max="2.0" val="1.923"></attnum>
    <attnum name="body height" unit="m" min="1.1" max="1.7" val="1.1"></attnum>
    <!-- collision bounding box -->
    <attnum name="overall length" unit="m" min="4.5" max="5.0" val="4.6"></attnum>
    <attnum name="overall width"  unit="m" min="1.2" max="2.0" val="1.93"></attnum>
    <attnum name="mass" unit="kg" min="900.0" max="1500" val="1250.0"></attnum>
    <attnum name="GC height" unit="m" min="0.15" max="0.5" val="0.30"></attnum>
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" min="0.3" max="0.7" val="0.55"></attnum>
    <attnum name="front right-left weight repartition" min="0.3" max="0.7" val="0.5"></attnum>
    <attnum name="rear right-left weight repartition" min="0.3" max="0.7" val="0.5"></attnum>
    <!-- used for inertia, indicates the good mass centering (lower values) -->
    <attnum name="mass repartition coefficient" min="0.2" max="1.0" val="0.7"></attnum>
    <attnum name="fuel tank" unit="l" min="30.0" max="100.0" val="100.0"></attnum>
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="80.0"></attnum>

  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" min="0.20" max="2.0" val="0.30"></attnum>
    <attnum name="front area" unit="m2" min="1.0" max="3.0" val="1.4"></attnum>
    <attnum name="Clift" min="0.0" max="1.0" val="1.0"></attnum>
  </section>

  <section name="Front Wing">
    <attnum name="area" unit="m2" min="0" max="0.5" val="0.25"></attnum>
    <attnum name="angle" unit="deg" min="0" max="30" val="30"></attnum>
    <attnum name="xpos" unit="m" min="1.0" max="2.5" val="2.21"></attnum>
    <attnum name="zpos" unit="m" min="0.1" max="0.5" val="0.1"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="area"  unit="m2" min="0" max="1.0" val="1.0"></attnum>
    <attnum name="angle" unit="deg" min="0" max="30" val="30"></attnum>
    <attnum name="xpos" unit="m" min="-2.5" max="-1.0" val="-2.05"></attnum>
    <attnum name="zpos" unit="m" min="0.1" max="1.5" val="1.0"></attnum>
  </section>

  <!-- Same engine for every one -->
  <section name="Engine">
    <attnum name="inertia"      unit="kg.m2" val="0.2423"></attnum>
    <attnum name="revs limiter" unit="rpm"   val="8500"></attnum>
    <attnum name="revs maxi"    unit="rpm"   val="10000"></attnum>
    <attnum name="tickover"     unit="rpm"   val="1000"></attnum>
    <attnum name="fuel cons factor"          val="1.2"></attnum>
    <section name="data points">
      <section name="1">
	<attnum name="rpm" unit="rpm" val="1000"></attnum>
	<attnum name="Tq"  unit="N.m" val="600"></attnum>
      </section>
      <section name="2">
	<attnum name="rpm" unit="rpm" val="6000"></attnum>
	<attnum name="Tq"  unit="N.m" val="744"></attnum>
      </section>
      <section name="3">
	<attnum name="rpm" unit="rpm" val="7000"></attnum>
	<attnum name="Tq"  unit="N.m" val="663"></attnum>
      </section>
      <section name="4">
	<attnum name="rpm" unit="rpm" val="7500"></attnum>
	<attnum name="Tq"  unit="N.m" val="619"></attnum>
      </section>
      <section name="5">
	<attnum name="rpm" unit="rpm" val="8000"></attnum>
	<attnum name="Tq"  unit="N.m" val="558"></attnum>
      </section>
      <section name="6">
	<attnum name="rpm" unit="rpm" val="10000"></attnum>
	<attnum name="Tq"  unit="N.m" val="357"></attnum>
      </section>
    </section>
  </section>

  <section name="Clutch">
    <!-- pressure plate -->
    <attnum name="inertia" unit="kg.m2" val="0.1150"></attnum>
  </section>

  <section name="Gearbox">
    <attnum name="shift time" unit="s" val="0.15"></attnum>
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"></attnum>
	<attnum name="inertia" val="0.0037"></attnum>
	<attnum name="efficiency" val="0.957"></attnum>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="2.66"></attnum>
	<attnum name="inertia" val="0.003"></attnum>
	<attnum name="efficiency" val="0.955"></attnum>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="1.78"></attnum>
	<attnum name="inertia" val="0.0037"></attnum>
	<attnum name="efficiency" val="0.957"></attnum>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.3"></attnum>
	<attnum name="inertia" val="0.0048"></attnum>
	<attnum name="efficiency" val="0.950"></attnum>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.00"></attnum>
	<attnum name="inertia" val="0.0064"></attnum>
	<attnum name="efficiency" val="0.983"></attnum>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val=".74"></attnum>
	<attnum name="inertia" val="0.0107"></attnum>
	<attnum name="efficiency" val="0.948"></attnum>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.50"></attnum>
	<attnum name="inertia" val="0.0150"></attnum>
	<attnum name="efficiency" val="0.940"></attnum>
      </section>
    </section>
  </section>

  <section name="Drivetrain">
    <!-- allowed values RWD, FWD, 4WD -->
    <attstr name="type"                 val="RWD"></attstr>
    <!-- drive shaft inertia -->
    <attnum name="inertia" unit="kg.m2" val="0.0091"></attnum>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="1" max="45" val="45"></attnum>
    <attnum name="max steer speed" unit="deg/s" min="1" max="360" val="120"></attnum>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.65"></attnum>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="10000"></attnum>
  </section>

  <section name="Front Axle">
    <attnum name="xpos" min="0.5" max="2.5" val="1.33"></attnum>
    <attnum name="inertia" unit="kg.m2"        val="0.0056"></attnum>
    <attnum name="roll center height" unit="m" min="0" max="0.5" val="0.012"></attnum>
  </section>

  <section name="Rear Axle">
    <attnum name="xpos" min="-2.5" max="-0.5" val="-1.25"></attnum>
    <attnum name="inertia" unit="kg.m2"        val="0.0080"></attnum>
    <attnum name="roll center height" unit="m" min="0" max="0.5" val="0.04"></attnum>
  </section>

  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="NONE" val="NONE"></attstr>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="SPOOL"></attstr>
    <attnum name="inertia" unit="kg.m2" val="0.0488"></attnum>
    <attnum name="ratio" min="0" max="5" val="3.07"></attnum>
    <attnum name="efficiency"           val="0.9625"></attnum>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ypos" unit="m" max="-0.5" min="-0.9" val="-0.81"></attnum>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" min="135" max="335" val="255"></attnum>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".40"></attnum>
    <attnum name="inertia" unit="kg.m2" val="1.2200"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="100" max="300" val="100"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
    <!-- Adherence -->
    <attnum name="stiffness" min="20.0" max="50.0" val="50.0"></attnum>
    <attnum name="dynamic friction" unit="%" min="80" max="70" val="80"></attnum>
    <attnum name="rolling resistance" val="0.02"></attnum>
    <attnum name="mu" min="0.05" max="1.5" val="1.5"></attnum>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ypos" unit="m" min="0.5" max="0.9" val="0.81"></attnum>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" min="135" max="335" val="255"></attnum>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".40"></attnum>
    <attnum name="inertia" unit="kg.m2" val="1.2200"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="100" max="300" val="100"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
    <!-- Adherence -->
    <attnum name="stiffness" min="20.0" max="50.0" val="50.0"></attnum>
    <attnum name="dynamic friction" unit="%" min="80" max="70" val="80"></attnum>
    <attnum name="rolling resistance" val="0.02"></attnum>
    <attnum name="mu" min="0.05" max="1.5" val="1.5"></attnum>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ypos" unit="m" max="-0.5" min="-0.9" val="-0.81"></attnum>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" min="135" max="335" val="330"></attnum>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".30"></attnum>
    <attnum name="inertia" unit="kg.m2" val="1.2200"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="100" max="300" val="100"></attnum>
    <attnum name="toe" unit="deg" min="0" max="0" val="0"></attnum>
    <!-- Adherence -->
    <attnum name="stiffness" min="20.0" max="50.0" val="50.0"></attnum>
    <attnum name="dynamic friction" unit="%" min="80" max="70" val="80"></attnum>
    <attnum name="rolling resistance" val="0.02"></attnum>
    <attnum name="mu" min="0.05" max="1.5" val="1.5"></attnum>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ypos" unit="m" min="0.5" max="0.9" val="0.81"></attnum>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" min="135" max="335" val="330"></attnum>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".30"></attnum>
    <attnum name="inertia" unit="kg.m2" val="1.2200"></attnum>
    <attnum name="mu" min="0.05" max="1.6" val="1.4"></attnum>
    <attnum name="ride height" unit="mm" min="50" max="300" val="100"></attnum>
    <attnum name="rolling resistance" val="0.0105"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="100" max="300" val="100"></attnum>
    <attnum name="toe" unit="deg" min="0" max="0" val="0"></attnum>
    <!-- Adherence -->
    <attnum name="stiffness" min="20.0" max="50.0" val="50.0"></attnum>
    <attnum name="dynamic friction" unit="%" min="80" max="70" val="80"></attnum>
    <attnum name="rolling resistance" val="0.02"></attnum>
    <attnum name="mu" min="0.05" max="1.5" val="1.5"></attnum>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2.5"></attnum>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2.5"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Front Right Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="380"></attnum>
    <attnum name="piston area"   unit="cm2"   val="50"></attnum>
    <attnum name="mu"                         val="0.45"></attnum>
    <attnum name="inertia"       unit="kg.m2" val="0.1241"></attnum>
  </section>

  <section name="Front Left Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="380"></attnum>
    <attnum name="piston area"   unit="cm2"   val="50"></attnum>
    <attnum name="mu"                         val="0.45"></attnum>
    <attnum name="inertia"       unit="kg.m2" val="0.1241"></attnum>
  </section>

  <section name="Rear Right Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="330"></attnum>
    <attnum name="piston area"   unit="cm2"   val="25"></attnum>
    <attnum name="mu"                         val="0.45"></attnum>
    <attnum name="inertia"       unit="kg.m2" val="0.0714"></attnum>
  </section>

  <section name="Rear Left Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="380" val="330"></attnum>
    <attnum name="piston area"   unit="cm2"   val="25"></attnum>
    <attnum name="mu"                         val="0.45"></attnum>
    <attnum name="inertia"       unit="kg.m2" val="0.0714"></attnum>
  </section>

</params>



