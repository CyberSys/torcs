<!DOCTYPE params SYSTEM "../../libs/tgf/params.dtd">

<params name="tutorial1" type="robotdef"> 
  <section name="Robots"> 
    <section name="index"> 
      <section name="1"> 
	<!-- Driver's name (short please) --> 
	<attstr name="name" val="Tutorial-1"></attstr> 
	<!-- Description (can be long) --> 
	<attstr name="desc" val="A tutorial1 robot that does nothing"></attstr> 
	<!-- Author's name --> 
	<attstr name="author" val="My Name"></attstr> 
	<!-- Car used to race --> 
	<attstr name="car name" val="cg-nascar-rwd"></attstr> 
	<!-- Car category --> 
	<attstr name="category" val="Track-RWD-GrA"></attstr> 
	<!-- Race number, should be unique for TORCS... --> 
	<attnum name="race number" val="99"></attnum> 
	<!-- Your color for car and board --> 
	<attnum name="red" val="1.0"></attnum> 
	<attnum name="green" val="1.0"></attnum> 
	<attnum name="blue" val="1.0"></attnum> 
      </section> 

      <section name="2"> 
	<attstr name="name" val="Tutorial-2"></attstr> 
	<attstr name="desc" val="A tutorial robot"></attstr> 
	<attstr name="author" val="My Name"></attstr> 
	<attstr name="car name" val="cg-nascar-rwd"></attstr> 
	<attstr name="category" val="Track-RWD-GrA"></attstr> 
	<attnum name="race number" val="99"></attnum> 
	<attnum name="red" val="1.0"></attnum> 
	<attnum name="green" val="1.0"></attnum> 
	<attnum name="blue" val="1.0"></attnum> 
      </section>

      <section name="3"> 
	<attstr name="name" val="Tutorial-3"></attstr> 
	<attstr name="desc" val="A tutorial robot"></attstr> 
	<attstr name="author" val="My Name"></attstr> 
	<attstr name="car name" val="cg-nascar-rwd"></attstr> 
	<attstr name="category" val="Track-RWD-GrA"></attstr> 
	<attnum name="race number" val="99"></attnum> 
	<attnum name="red" val="1.0"></attnum> 
	<attnum name="green" val="1.0"></attnum> 
	<attnum name="blue" val="1.0"></attnum> 
      </section>

    </section> 
  </section> 
</params>
