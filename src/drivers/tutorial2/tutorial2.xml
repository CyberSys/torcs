<!DOCTYPE params SYSTEM "../../libs/tgf/params.dtd">

<params name="tutorial1" type="robotdef"> 
  <section name="Robots"> 
    <section name="index"> 
      <section name="1"> 
	<!-- Driver's name (short please) --> 
	<attstr name="name" val="Tutorial2"/> 
	<!-- Description (can be long) --> 
	<attstr name="desc" val="Tutorial1 robot"/> 
	<!-- Author's name --> 
	<attstr name="author" val="My Name"/> 
	<!-- Car used to race --> 
	<attstr name="car name" val="cg-nascar-rwd"/> 
	<!-- Race number, should be unique for TORCS... --> 
	<attnum name="race number" val="98"/> 
	<!-- Your color for car and board --> 
	<attnum name="red" val="1.0"/> 
	<attnum name="green" val="1.0"/> 
	<attnum name="blue" val="1.0"/> 
      </section> 
    </section> 
  </section> 
</params>
