<!DOCTYPE params SYSTEM "../../../../libs/tgf/params.dtd">

<params name="TORCS FREE" type="template" mode="mw">


  <section name="Graphic Objects">
    <attstr name="tachometer texture" val="rpm7000.rgb"></attstr>
    <attnum name="tachometer min value" val="0" unit="rpm"></attnum>
    <attnum name="tachometer max value" val="8000" unit="rpm"></attnum>
    <attstr name="speedometer texture" val="speed240.rgb"></attstr>
    <attnum name="speedometer min value" val="0" unit="km/h"></attnum>
    <attnum name="speedometer max value" val="240" unit="km/h"></attnum>
  </section>

  <section name="Car">
    <attstr name="category" val="free"></attstr>    
    <attnum name="body length" unit="m" min="1.5" max="25.0" val="5"></attnum>
    <attnum name="body width" unit="m" min="0.5" max="4.0" val="2"></attnum>
    <attnum name="body height" unit="m" min="0.5" max="10.0" val="1.4"></attnum>
    <!-- collision bounding box -->
    <attnum name="overall length" unit="m" min="1.5" max="25.0" val="5"></attnum>
    <attnum name="overall width"  unit="m" min="0.5" max="4.0" val="2"></attnum>
    <attnum name="mass" unit="kg" min="400.0" max="15000" val="650.0"></attnum>
    <attnum name="GC height" unit="m" min="0.0" max="5.0" val="0.25"></attnum>
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" min="0.1" max="0.9" val="0.5"></attnum>
    <attnum name="front right-left weight repartition" min="0.1" max="0.9" val="0.5"></attnum>
    <attnum name="rear right-left weight repartition" min="0.1" max="0.9" val="0.5"></attnum>
    <!-- used for inertia, indicates the good mass centering (lower values) -->
    <attnum name="mass repartition coefficient" min="0.1" max="10.0" val="0.5"></attnum>
    <attnum name="fuel tank" unit="l" min="20.0" max="1000.0" val="100.0"></attnum>
    <attnum name="initial fuel" unit="l" min="1.0" max="1000.0" val="80.0"></attnum>
  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" min="0.20" max="5.0" val="0.30"></attnum>
    <attnum name="front area" unit="m2" min="0.75" max="40.0" val="2.0"></attnum>
    <attnum name="Clift" min="-1.0" max="2.0" val="1.0"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="area"  unit="m2" min="0" max="2.0" val="1.0"></attnum>
    <attnum name="angle" unit="deg" min="0" max="80" val="10"></attnum>
    <attnum name="xpos" unit="m" min="-12.5" max="0.0" val="-2.3"></attnum>
    <attnum name="zpos" unit="m" min="0.0" max="10.0" val=".6"></attnum>
  </section>

  <section name="Engine">
    <attnum name="inertia"      unit="kg.m2" val="0.20"></attnum>
    <attnum name="revs limiter" unit="rpm"   val="6500"></attnum>
    <attnum name="revs maxi"    unit="rpm"   val="7000"></attnum>
    <attnum name="tickover"     unit="rpm"   val="1000"></attnum>
    <attnum name="fuel cons factor"          val="1.2"></attnum>
    <section name="data points">
      <section name="1">
	<attnum name="rpm" unit="rpm" val="1000"></attnum>
	<attnum name="Tq"  unit="N.m" val="300"></attnum>
      </section>
      <section name="2">
	<attnum name="rpm" unit="rpm" val="3500"></attnum>
	<attnum name="Tq"  unit="N.m" val="300"></attnum>
      </section>
      <section name="3">
	<attnum name="rpm" unit="rpm" val="4000"></attnum>
	<attnum name="Tq"  unit="N.m" val="350"></attnum>
      </section>
      <section name="4">
	<attnum name="rpm" unit="rpm" val="5000"></attnum>
	<attnum name="Tq"  unit="N.m" val="350"></attnum>
      </section>
      <section name="5">
	<attnum name="rpm" unit="rpm" val="5500"></attnum>
	<attnum name="Tq"  unit="N.m" val="350"></attnum>
      </section>
      <section name="6">
	<attnum name="rpm" unit="rpm" val="6000"></attnum>
	<attnum name="Tq"  unit="N.m" val="320"></attnum>
      </section>
      <section name="7">
	<attnum name="rpm" unit="rpm" val="7000"></attnum>
	<attnum name="Tq"  unit="N.m" val="300"></attnum>
      </section>
    </section>
  </section>

  <section name="Clutch">
    <!-- pressure plate -->
    <attnum name="inertia" val="0.080" unit="kg.m2" min="0.05" max="0.2"></attnum>
  </section>

  <section name="Gearbox">
    <attnum name="shift time" val="0.1" unit="s" min="0.01" max="1.0"></attnum>
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-5" max="0" val="-2.0"></attnum>
	<attnum name="inertia" val="0.0037" min="0.001" max="0.1"></attnum>
	<attnum name="efficiency" val="0.957" min="0.5" max="1.0"></attnum>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="10" val="3.82"></attnum>
	<attnum name="inertia" val="0.003" min="0.001" max="0.1"></attnum>
	<attnum name="efficiency" val="0.955" min="0.5" max="1.0"></attnum>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="10" val="2.15"></attnum>
	<attnum name="inertia" val="0.0037" min="0.001" max="0.1"></attnum>
	<attnum name="efficiency" val="0.957" min="0.5" max="1.0"></attnum>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="10" val="1.56"></attnum>
	<attnum name="inertia" val="0.0048" min="0.001" max="0.1"></attnum>
	<attnum name="efficiency" val="0.950" min="0.5" max="1.0"></attnum>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="10" val="1.21"></attnum>
	<attnum name="inertia" val="0.0064" min="0.001" max="0.1"></attnum>
	<attnum name="efficiency" val="0.983" min="0.5" max="1.0"></attnum>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="10" val="0.97"></attnum>
	<attnum name="inertia" val="0.0107" min="0.001" max="0.1"></attnum>
	<attnum name="efficiency" val="0.948" min="0.5" max="1.0"></attnum>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="10" val="0.78"></attnum>
	<attnum name="inertia" val="0.0150" min="0.001" max="0.1"></attnum>
	<attnum name="efficiency" val="0.940" min="0.5" max="1.0"></attnum>
      </section>
    </section>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="1" max="60" val="45"></attnum>
    <attnum name="max steer speed" unit="deg/s" min="1" max="360" val="120"></attnum>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.0" max="1.0" val="0.6"></attnum>
    <attnum name="max pressure" unit="kPa" min="100" max="1500000" val="3000"></attnum>
  </section>

  <section name="Front Axle">
    <attnum name="xpos" min="0.0" max="12.5" val="1.58"></attnum>
    <attnum name="inertia" unit="kg.m2" val="0.0056" min="0.001" max="0.1"></attnum>
    <attnum name="roll center height" unit="m" min="-1.0" max="5.0" val="0.012"></attnum>
  </section>

  <section name="Rear Axle">
    <attnum name="xpos" min="-12.5" max="0.0" val="-1.35"></attnum>
    <attnum name="inertia" unit="kg.m2" val="0.0080" min="0.001" max="0.1"></attnum>
    <attnum name="roll center height" unit="m" min="-1.0" max="5.0" val="0.04"></attnum>
  </section>


  <section name="Drivetrain">
    <!-- allowed values RWD, FWD, 4WD -->
    <attstr name="type" in="RWD,FWD,RWD" val="4WD"></attstr>
    <!-- drive shaft inertia -->
    <attnum name="inertia" unit="kg.m2" val="0.0091" min="0.001" max="0.1"></attnum>
  </section>

  <section name="Central Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="NONE,SPOOL,FREE,LIMITED SLIP,VISCOUS COUPLER" val="VISCOUS COUPLER"></attstr>
    <attnum name="min torque bias" val="50" unit="%"></attnum>
    <attnum name="max torque bias" val="60" unit="%"></attnum>
    <attnum name="inertia" unit="kg.m2" val="0.0488" min="0.001" max="0.1"></attnum>
    <attnum name="ratio" min="0" max="10" val="4.5"></attnum>
    <attnum name="efficiency" val="0.9625" min="0.5" max="1.0"></attnum>
  </section>

  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="NONE,SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"></attstr>
    <attnum name="inertia" unit="kg.m2" val="0.0488" min="0.001" max="0.1"></attnum>
    <attnum name="ratio" min="0" max="10" val="1.0"></attnum>
    <attnum name="efficiency" val="0.9625" min="0.5" max="1.0"></attnum>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="NONE,SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"></attstr>
    <attnum name="inertia" unit="kg.m2" val="0.0488" min="0.001" max="0.1"></attnum>
    <attnum name="ratio" min="0" max="10" val="1.0"></attnum>
    <attnum name="efficiency" val="0.9625" min="0.5" max="1.0"></attnum>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ypos" unit="m" max="-0.1" min="-2.0" val="-0.90"></attnum>
    <attnum name="rim diameter" unit="in" min="12" max="30" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" min="125" max="500" val="195"></attnum>
    <attnum name="tire height-width ratio" min="0.1" max="1.0" val=".35"></attnum>
    <attnum name="inertia" val="1.2200" unit="kg.m2" min="0.5" max="4.0"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="50" max="2000" val="150"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
    <!-- Adherence -->
    <attnum name="stiffness" min="10.0" max="50.0" val="10.0"></attnum>
    <attnum name="dynamic friction" unit="%" min="50" max="100" val="100"></attnum>
    <attnum name="rolling resistance" val="0.02" min="0.01" max="0.1"></attnum>
    <attnum name="mu" min="0.05" max="5.0" val="1.5"></attnum>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ypos" unit="m" min="0.1" max="2.0" val="0.90"></attnum>
    <attnum name="rim diameter" unit="in" min="12" max="30" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" min="125" max="500" val="195"></attnum>
    <attnum name="tire height-width ratio" min="0.1" max="1.0" val=".35"></attnum>
    <attnum name="inertia" val="1.2200" unit="kg.m2" min="0.5" max="4.0"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="50" max="2000" val="150"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
    <!-- Adherence -->
    <attnum name="stiffness" min="10.0" max="50.0" val="10.0"></attnum>
    <attnum name="dynamic friction" unit="%" min="50" max="100" val="100"></attnum>
    <attnum name="rolling resistance" val="0.02" min="0.01" max="0.1"></attnum>
    <attnum name="mu" min="0.05" max="5.0" val="1.5"></attnum>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ypos" unit="m" max="-0.1" min="-2.0" val="-0.90"></attnum>
    <attnum name="rim diameter" unit="in" min="12" max="30" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" min="125" max="500" val="235"></attnum>
    <attnum name="tire height-width ratio" min="0.1" max="1.0" val=".30"></attnum>
    <attnum name="inertia" val="1.2200" unit="kg.m2" min="0.5" max="4.0"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="50" max="2000" val="160"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
    <!-- Adherence -->
    <attnum name="stiffness" min="10.0" max="50.0" val="10.0"></attnum>
    <attnum name="dynamic friction" unit="%" min="50" max="100" val="100"></attnum>
    <attnum name="rolling resistance" val="0.02" min="0.01" max="0.1"></attnum>
    <attnum name="mu" min="0.05" max="5.0" val="1.5"></attnum>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ypos" unit="m" min="0.1" max="2.0" val="0.90"></attnum>
    <attnum name="rim diameter" unit="in" min="12" max="30" val="18.0"></attnum>
    <attnum name="tire width" unit="mm" min="125" max="500" val="235"></attnum>
    <attnum name="tire height-width ratio" min="0.1" max="1.0" val=".30"></attnum>
    <attnum name="inertia" val="1.2200" unit="kg.m2" min="0.5" max="4.0"></attnum>
    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="50" max="2000" val="160"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
    <!-- Adherence -->
    <attnum name="stiffness" min="10.0" max="50.0" val="10.0"></attnum>
    <attnum name="dynamic friction" unit="%" min="50" max="100" val="100"></attnum>
    <attnum name="rolling resistance" val="0.02" min="0.01" max="0.1"></attnum>
    <attnum name="mu" min="0.05" max="5.0" val="1.5"></attnum>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2"></attnum>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.5" val="0.35"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.5" val="0.35"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3200"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.5" val="0.3"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3200"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.5" val="0.3"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="50"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
  </section>

  <section name="Front Right Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="500" val="380"></attnum>
    <attnum name="piston area" val="50" unit="cm2" min="25" max="200"></attnum>
    <attnum name="mu" val="0.45" min="0.25" max="0.5"></attnum>
    <attnum name="inertia" val="0.1241" unit="kg.m2" min="0.05" max="0.3"></attnum>
  </section>

  <section name="Front Left Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="500" val="380"></attnum>
    <attnum name="piston area" val="50" unit="cm2" min="25" max="200"></attnum>
    <attnum name="mu" val="0.45" min="0.25" max="0.5"></attnum>
    <attnum name="inertia" val="0.1241" unit="kg.m2" min="0.05" max="0.3"></attnum>
  </section>

  <section name="Rear Right Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="500" val="380"></attnum>
    <attnum name="piston area" val="50" unit="cm2" min="25" max="200"></attnum>
    <attnum name="mu" val="0.45" min="0.25" max="0.5"></attnum>
    <attnum name="inertia" val="0.1241" unit="kg.m2" min="0.05" max="0.3"></attnum>
  </section>

  <section name="Rear Left Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="500" val="380"></attnum>
    <attnum name="piston area" val="50" unit="cm2" min="25" max="200"></attnum>
    <attnum name="mu" val="0.45" min="0.25" max="0.5"></attnum>
    <attnum name="inertia" val="0.1241" unit="kg.m2" min="0.05" max="0.3"></attnum>
  </section>


</params>


