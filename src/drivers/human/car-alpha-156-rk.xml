<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Alfa Romeo 156" type="template">

  <section name="Car">
    <attnum name="initial fuel" min="1.0" max="80.0" unit="l" val="50.0"/>
  </section>

  <section name="Front Wing">
    <attnum name="angle" min="0" max="30" unit="deg" val="0"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" min="0" max="40" unit="deg" val="15"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"/>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="3.82"/>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="2.15"/>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.56"/>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.21"/>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.97"/>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.75"/>
      </section>
    </section>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.7"/>
    <attnum name="max pressure" min="1000" max="500000" unit="kPa" val="50000"/>
  </section>

  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP,VISCOUS COUPLER" val="LIMITED SLIP"/>
    <attnum name="ratio" min="0" max="10" val="4.444"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="rim diameter" min="15" max="17" unit="in" val="17.0"/>
    <attnum name="tire width" min="135" max="250" unit="mm" val="250"/>
    <attnum name="tire height-width ratio" min="0.3" max="0.5" val=".35"/>
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="stiffness" min="1.0" max="30.0" val="20.0"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-2"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="rim diameter" min="15" max="17" unit="in" val="17.0"/>
    <attnum name="tire width" min="135" max="250" unit="mm" val="250"/>
    <attnum name="tire height-width ratio" min="0.3" max="0.5" val=".35"/>
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="stiffness" min="1.0" max="30.0" val="20.0"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-2"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="rim diameter" min="15" max="17" unit="in" val="17.0"/>
    <attnum name="tire width" min="135" max="250" unit="mm" val="250"/>
    <attnum name="tire height-width ratio" min="0.3" max="0.5" val=".35"/>
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="stiffness" min="1.0" max="30.0" val="20.0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="rim diameter" min="15" max="17" unit="in" val="17.0"/>
    <attnum name="tire width" min="135" max="250" unit="mm" val="250"/>
    <attnum name="tire height-width ratio" min="0.3" max="0.5" val=".35"/>
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="stiffness" min="1.0" max="30.0" val="20.0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" min="0" max="5000" unit="lbs/in" val="0"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2.5"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" min="0" max="5000" unit="lbs/in" val="0"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2.5"/>
  </section>


</params>


