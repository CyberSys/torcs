<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Alfa Romeo 155 DTM" mode="mw">

<!-- The car 3D model is used with the authorization of ViPeR (viper@mttestdriver.com) -->


  <section name="Car">
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" min="0.4" max="0.6" val="0.5"></attnum>
    <attnum name="front right-left weight repartition" min="0.4" max="0.6" val="0.5"></attnum>
    <attnum name="rear right-left weight repartition" min="0.4" max="0.6" val="0.5"></attnum>
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="30.0"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="area"  unit="m2" val="0.8"></attnum>
    <attnum name="angle" unit="deg" min="0" max="30" val="5"></attnum>
    <attnum name="xpos" unit="m" val="-1.94"></attnum>
    <attnum name="zpos" unit="m" val="1.03"></attnum>
  </section>


  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-5" max="0" val="-3.4"></attnum>
	<attnum name="efficiency" val="0.9"></attnum>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="2.533333"></attnum>
	<attnum name="efficiency" val="1.0"></attnum>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="1.650000"></attnum>
	<attnum name="efficiency" val="1.0"></attnum>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.208333"></attnum>
	<attnum name="efficiency" val="1.0"></attnum>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="0.962963"></attnum>
	<attnum name="efficiency" val="1.0"></attnum>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.766667"></attnum>
	<attnum name="efficiency" val="1.0"></attnum>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.65"></attnum>
	<attnum name="efficiency" val="1.0"></attnum>
      </section>
    </section>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="1" max="45" val="45"></attnum>
    <attnum name="max steer speed" unit="deg/s" min="1" max="360" val="120"></attnum>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.7"></attnum>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="11000"></attnum>
  </section>

  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="FREE"></attstr>
    <attnum name="ratio" min="0" max="15" val="1.0"></attnum>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"></attstr>
    <attnum name="ratio" min="0" max="15" val="1.0"></attnum>
  </section>

  <section name="Central Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="NONE,SPOOL,FREE,LIMITED SLIP,VISCOUS COUPLER" val="VISCOUS COUPLER"></attstr>
    <attnum name="min torque bias" val="50" unit="%"></attnum>
    <attnum name="max torque bias" val="70" unit="%"></attnum>
    <attnum name="ratio" min="0" max="10" val="6.5"></attnum>
  </section>

  <section name="Front Right Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"></attnum>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"></attnum>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3000"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3000"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3000"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3000"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="40"></attnum>
  </section>

</params>


