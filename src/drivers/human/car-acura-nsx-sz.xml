<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Acura NSX type S-Zero" type="parameter" mode="mw">

  <section name="Car">
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="30.0"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="20"/>
 </section>


  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-5" max="0" val="-3.4"/>
	<attnum name="efficiency" val="0.9"/>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="2.533333"/>
	<attnum name="efficiency" val="1.0"/>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="1.650000"/>
	<attnum name="efficiency" val="1.0"/>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.208333"/>
	<attnum name="efficiency" val="1.0"/>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="0.962963"/>
	<attnum name="efficiency" val="1.0"/>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.766667"/>
	<attnum name="efficiency" val="1.0"/>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.688454"/>
	<attnum name="efficiency" val="1.0"/>
      </section>
    </section>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="1" max="45" val="45"/>
    <attnum name="max steer speed" unit="deg/s" min="1" max="360" val="120"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.1" max="0.7" val="0.65"/>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="10000"/>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE" val="FREE"/>
    <attnum name="ratio" min="0" max="15" val="4.60000"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
    <attnum name="suspension course" unit="m" val="0.25"/>
    <attnum name="bellcrank" val="1.5"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
    <attnum name="suspension course" unit="m" val="0.25"/>
    <attnum name="bellcrank" val="1.5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"/>
    <attnum name="suspension course" unit="m" val="0.25"/>
    <attnum name="bellcrank" val="1.5"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="800"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="800"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"/>
    <attnum name="suspension course" unit="m" val="0.25"/>
    <attnum name="bellcrank" val="1.5"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="800"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="800"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"/>
    <attnum name="suspension course" unit="m" val="0.25"/>
    <attnum name="bellcrank" val="1.5"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="800"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="1000"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5500"/>
    <attnum name="suspension course" unit="m" val="0.25"/>
    <attnum name="bellcrank" val="1.5"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="800"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="1000"/>
  </section>

</params>


