<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="NASCAR RWD" type="template">

  <section name="Graphic Objects">
    <attstr name="env" val="cg-nascar-rwd.ac"></attstr>
    <attstr name="wheel texture" val="tex-wheel.rgb"></attstr>

    <section name="Ranges">
      <section name="1">
	<attnum name="distance" val="500" unit="km"></attnum>
	<attstr name="car" val="cg-nascar-rwd.ac"></attstr>
      </section>
    </section>
  </section>


  <section name="Car">
    <attstr name="category" val="torcs-r-a"></attstr>
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="28.0"></attnum>
  </section>

  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="30"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="20"></attnum>
  </section>

  <!-- Same engine for every one -->

  <section name="Gearbox">
    <attnum name="shift time" val="0.25" unit="s"></attnum>
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"></attnum>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="3.3"></attnum>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="2.5"></attnum>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.8"></attnum>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.5"></attnum>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="1.25"></attnum>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="1.05"></attnum>
      </section>
    </section>
  </section>


  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"></attstr>
    <attnum name="ratio" min="0" max="10" val="3.444"></attnum>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.4"></attnum>
  </section>

 
  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="dynamic friction" unit="%" min="80" max="70" val="80"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1.8"></attnum>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1.8"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="60"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="60"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="75"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="75"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="60"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="60"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="75"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="75"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="60"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="60"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="75"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="75"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2500"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="60"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="60"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="75"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="75"></attnum>
  </section>

  <section name="berniw private">
    <!--attnum name="pitentry" val="1620"></attnum-->
	<!--attnum name="pitexit" val="300"></attnum-->
  </section>


</params>






