<!DOCTYPE params SYSTEM "../../../libs/tgf/params.dtd">

<params name="Tanhoj" type="template">
  <section name="Car">
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="100.0"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="30"></attnum>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"></attnum>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="3.0"></attnum>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="2.0"></attnum>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.56"></attnum>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.21"></attnum>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="1.05"></attnum>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.9"></attnum>
      </section>
    </section>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" val="LIMITED SLIP"></attstr>
    <attnum name="ratio" val="3.5"></attnum>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.31"></attnum>
    <attnum name="max pressure" unit="kPa" val="13000"></attnum>
  </section>

  <section name="Front Right Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Front Left Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"></attnum>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"></attnum>
  </section>

  <section name="Rear Right Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"></attnum>
    <attnum name="rim diameter" unit="in" min="16" max="20" val="20.0"></attnum>
  </section>

  <section name="Rear Left Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"></attnum>
    <attnum name="rim diameter" unit="in" min="16" max="20" val="20.0"></attnum>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1"></attnum>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1.5"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="6000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1.0"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="330"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="350"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="340"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="360"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="6000"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1.0"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="330"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="350"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="340"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="360"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="6850"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1.0"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="300"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="380"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="360"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="390"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="6850"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="1.0"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="300"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="380"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="360"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="390"></attnum>
  </section>

</params>





