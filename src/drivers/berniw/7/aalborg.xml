<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Lotus GT1" type="template">

<!-- The car 3D model is used with the authorization of ViPeR (viper@mttestdriver.com) -->

 

  <section name="Car">
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="60.0"></attnum>
  </section>

  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="30"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="12"></attnum>
  </section>

  <section name="Gearbox">
    <attnum name="shift time" unit="s" val="0.15"></attnum>
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"></attnum>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="3.3"></attnum>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="2.5"></attnum>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.8"></attnum>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.5"></attnum>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="1.25"></attnum>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="1.05"></attnum>
      </section>
    </section>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.38"></attnum>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="10000"></attnum>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2.5"></attnum>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"></attnum>
    <attnum name="bellcrank" min="1" max="5" val="2.5"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2200"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.12"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2200"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.12"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.2"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2200"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.15"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2200"></attnum>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.15"></attnum>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="70"></attnum>
  </section>

</params>






