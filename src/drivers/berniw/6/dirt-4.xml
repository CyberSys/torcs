<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Alfa Romeo 155 DTM" type="template">

<!-- The car 3D model is used with the authorization of ViPeR (viper@mttestdriver.com) -->

  <section name="Car">
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="50.0"></attnum>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="15"></attnum>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-5" max="0" val="-3.4"></attnum>
	<attnum name="efficiency" val="0.9"></attnum>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="3.0"></attnum>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="2.2"></attnum>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.6"></attnum>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.2"></attnum>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.88"></attnum>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.67"></attnum>
      </section>
    </section>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.42"></attnum>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="11000"></attnum>
  </section>


  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2500"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2500"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2500"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2500"></attnum>
    <attnum name="suspension course" unit="m" val="0.25"></attnum>
    <attnum name="bellcrank" val="1.5"></attnum>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"></attnum>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="80"></attnum>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="100"></attnum>
  </section>
</params>





