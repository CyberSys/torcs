<!DOCTYPE params SYSTEM "../../../libs/tgf/params.dtd">

<params name="Tanhoj" type="template">
  <section name="Car">
    <!-- weight bias -->
    <attnum name="front right-left weight repartition" min="0.4" max="0.6" val="0.45"/>
    <attnum name="rear right-left weight repartition" min="0.4" max="0.6" val="0.55"/>
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="100.0"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"/>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="3.0"/>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="2.0"/>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.56"/>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.21"/>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.97"/>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.75"/>
      </section>
    </section>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" val="LIMITED SLIP"/>
    <attnum name="ratio" val="4.8"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.52"/>
    <attnum name="max pressure" unit="kPa" val="25000"/>
  </section>

  <section name="Front Right Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Front Left Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Rear Right Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="rim diameter" unit="in" min="16" max="20" val="20.0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Rear Left Wheel">
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="rim diameter" unit="in" min="16" max="20" val="20.0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="1"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="1.5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="1.3"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="130"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="150"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="140"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="160"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="5000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="1.3"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="130"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="150"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="140"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="160"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="6000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="1.3"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="200"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="180"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="160"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="190"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="6000"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="1.3"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="200"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="180"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="160"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="190"/>
  </section>

</params>



