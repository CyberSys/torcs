<!DOCTYPE params SYSTEM "../../../libs/tgf/params.dtd">

<params name="quick race" type="param" mode="mw">

  <section name="Race">
    <!-- comma separated section of allowed categories ("*" means every categories can race) -->
    <attstr name="categories" val="*"></attstr>
    <attstr name="track" val="e-track-3"></attstr>
    <attnum name="laps"  val="20"></attnum>
  </section>

  <section name="Starting Grid">
    <attnum name="rows"                      val="2"></attnum>
    <attnum name="distance to start"         val="25"></attnum>
    <attnum name="distance between columns"  val="20"></attnum>
    <attnum name="offset within a column"    val="10"></attnum>
    <attnum name="initial speed" unit="km/h" val="0"></attnum>
    <attnum name="initial height" unit="m"   val="1.0"></attnum>
  </section>

  <section name="Modules">
    <attstr name="track"     val="track"></attstr>
    <attstr name="simu"      val="simuv2"></attstr>
    <attstr name="graphic"   val="ssggraph"></attstr>
  </section>


  <section name="Drivers">
    <attnum name="ndrivers" val="15.000000"></attnum>
    <attstr name="focused module" val="lliaw"></attstr>
    <attnum name="focused idx" val="1.000000"></attnum>

    <section name="drivers">


      <section name="1">
	<attnum name="idx" val="1.000000"></attnum>
	<attstr name="module" val="K1999"></attstr>
      </section>

      <section name="2">
	<attnum name="idx" val="1.000000"></attnum>
	<attstr name="module" val="tita"></attstr>
      </section>

      <section name="3">
	<attnum name="idx" val="1.000000"></attnum>
	<attstr name="module" val="lliaw"></attstr>
      </section>

      <section name="4">
	<attnum name="idx" val="1.000000"></attnum>
	<attstr name="module" val="tanhoj"></attstr>
      </section>

      <section name="5">
	<attnum name="idx" val="1.000000"></attnum>
	<attstr name="module" val="cylos1"></attstr>
      </section>

      <section name="6">
	<attnum name="idx" val="8.000000"></attnum>
	<attstr name="module" val="damned"></attstr>
      </section>

      <section name="7">
	<attnum name="idx" val="5.000000"></attnum>
	<attstr name="module" val="damned"></attstr>
      </section>

      <section name="8">
