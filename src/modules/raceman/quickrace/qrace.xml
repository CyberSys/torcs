<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE params SYSTEM "../../dtd/params.dtd">

<params name="quick race" type="param" mode="mw">

    <section name="Race">
      <attstr name="categories" val="*"></attstr>
      <attstr name="track" val="wheel-1"></attstr>
      <attnum name="laps" val="25.000000"></attnum>
    </section>

    <section name="Starting Grid">
      <attnum name="rows" val="2.000000"></attnum>
      <attnum name="distance to start" val="25.000000"></attnum>
      <attnum name="distance between columns" val="20.000000"></attnum>
      <attnum name="offset within a column" val="10.000000"></attnum>
      <attnum name="initial speed" val="0.000000"></attnum>
      <attnum name="initial height" val="1.000000"></attnum>
    </section>

    <section name="Modules">
      <attstr name="track" val="track"></attstr>
      <attstr name="simu" val="simuv2"></attstr>
      <attstr name="graphic" val="ssggraph"></attstr>
    </section>

    <section name="Drivers">
      <attnum name="ndrivers" val="10.000000"></attnum>
      <attstr name="focused module" val="berniw"></attstr>
      <attnum name="focused idx" val="9.000000"></attnum>

      <section name="drivers">

        <section name="1">
          <attnum name="idx" val="9.000000"></attnum>
          <attstr name="module" val="berniw"></attstr>
        </section>

        <section name="2">
          <attnum name="idx" val="1.000000"></attnum>
          <attstr name="module" val="berniw"></attstr>
        </section>

        <section name="3">
          <attnum name="idx" val="7.000000"></attnum>
          <attstr name="module" val="berniw"></attstr>
        </section>

        <section name="4">
          <attnum name="idx" val="1.000000"></attnum>
          <attstr name="module" val="inferno"></attstr>
        </section>

        <section name="5">
          <attnum name="idx" val="1.000000"></attnum>
          <attstr name="module" val="tanhoj"></attstr>
        </section>

        <section name="6">
          <attnum name="idx" val="8.000000"></attnum>
          <attstr name="module" val="berniw"></attstr>
        </section>

        <section name="7">
          <attnum name="idx" val="1.000000"></attnum>
          <attstr name="module" val="tita"></attstr>
        </section>

        <section name="8">
          <attnum name="idx" val="1.000000"></attnum>
          <attstr name="module" val="lliaw"></attstr>
        </section>

        <section name="9">
          <attnum name="idx" val="2.000000"></attnum>
          <attstr name="module" val="K1999"></attstr>
        </section>

        <section name="10">
          <attnum name="idx" val="1.000000"></attnum>
          <attstr name="module" val="cylos1"></attstr>
        </section>
      </section>
    </section>

</params>
